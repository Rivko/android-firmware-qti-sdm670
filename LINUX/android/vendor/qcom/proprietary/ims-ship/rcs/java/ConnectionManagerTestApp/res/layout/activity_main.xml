<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <Button
        android:id="@+id/CreateManager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Create_Manager" />
    <Button
        android:id="@+id/ActivateSub0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Sub0" />
    <Button
        android:id="@+id/ActivateSub1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Sub1" />
    <Button
        android:id="@+id/CreateSMConn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Create_SMConn" />
    <Button
        android:id="@+id/CreateIMConn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Create_IMConn" />
    <Button
        android:id="@+id/CreateInvalidConn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Create_InvalidConn" />
    <Button
        android:id="@+id/TriggerRegistration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Trigger_Registration" />
    <Button
        android:id="@+id/TriggerDeRegistration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Trigger_DeRegistration" />
    <Button
        android:id="@+id/SendSMMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Send_SMMessage" />
    <Button
        android:id="@+id/SendIMMessage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Send_IMMessage" />
    <Button
        android:id="@+id/CloseSMConn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Close_SMConn" />
    <Button
        android:id="@+id/CloseIMConn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Close_IMConn" />
    <Button
        android:id="@+id/CloseManager"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Close_Manager" />
    <Button
        android:id="@+id/GetConfiguration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Get_Configuration" />
    <Button
        android:id="@+id/MethodResponse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Method_Response" />
    <Button
        android:id="@+id/socketSend"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Socket_Send" />
    <Button
        android:id="@+id/regRestoration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Reg_Restoration" />
    <Button
        android:id="@+id/autoconfig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Autoconfig" />
    <Button
        android:id="@+id/setStatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/setStatus" />
    <Button
        android:id="@+id/sendMsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/sendMsg" />
</LinearLayout>
</ScrollView>
