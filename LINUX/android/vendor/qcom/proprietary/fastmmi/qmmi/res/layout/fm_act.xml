<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/full_layout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/case_top"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/case_title"
            layout="@layout/case_title" />

        <Button
            android:id="@+id/fm_search"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/case_title"
            android:text="@string/fm_search"
            android:textAppearance="?android:attr/textAppearanceMedium"></Button>
    </LinearLayout>

    <include
        android:id="@+id/case_bottom"
        layout="@layout/case_bottom"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

</RelativeLayout>
