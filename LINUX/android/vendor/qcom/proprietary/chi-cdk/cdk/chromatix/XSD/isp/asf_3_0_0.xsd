<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="ispglobalelements.xsd"/>
  <xs:element name="asf30_ipe" type="chromatix_asf30Type">
    <xs:annotation>
      <xs:documentation>asf30 module parameters</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="chromatix_asf30Type">
    <xs:sequence>
      <xs:element name="enable_section">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="asf_enable" type="enable_flag_type" default="1"/>
            <xs:element name="skin_enable" default="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="enable bit of skin color based noise reduction"/>
                    <xs:attribute name="default_value" type="xs:int" default="0"/>
                    <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                    <xs:attribute name="length" type="xs:int" fixed="1"/>
                    <xs:attribute name="conversion" type="xs:string"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="if face_enable = 1 and skin_enable = 1, skin color noise reduction is applied on face regions; if face_enable  = 0 and skin_enable=1, skin color noise reduction is applied on both face regions and non-face regions"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="chroma_gradient_enable" default="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="enable bit of chroma gradient statistics"/>
                    <xs:attribute name="default_value" type="xs:int" default="0"/>
                    <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                    <xs:attribute name="length" type="xs:int" fixed="1"/>
                    <xs:attribute name="conversion" type="xs:string"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="module_version">
        <xs:complexType>
          <xs:attribute name="major_revision" type="xs:int" default="3"/>
          <xs:attribute name="minor_revision" type="xs:int" default="0"/>
          <xs:attribute name="incr_revision" type="xs:int" default="0"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_variables">
        <xs:complexType>
          <xs:sequence minOccurs="4" maxOccurs="4">
            <xs:element name="control_var_type">
              <xs:simpleType>
                <xs:restriction base="control_var_type">
                  <xs:pattern value="control_drc_gain"/>
                  <xs:pattern value="control_exp_time_ratio"/>
                  <xs:pattern value="control_aec_exp_sensitivity_ratio"/>
                  <xs:pattern value="control_lux_idx"/>
                  <xs:pattern value="control_gain"/>
                  <xs:pattern value="control_total_scale_ratio"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="default_value" type="xs:string" default="control_total_scale_ratio, control_drc_gain, control_aec_exp_sensitivity_ratio, control_gain"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_method">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="aec_exp_control" type="tuning_control_aec_type" default="control_gain"/>
            <xs:element name="aec_hdr_control" type="tuning_control_aec_hdr_type" default="control_aec_exp_sensitivity_ratio"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="dynamic_enable_triggers">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="asf_enable">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="enable">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="enable_flag_type">
                          <xs:attribute name="type" default="int"/>
                          <xs:attribute name="description" type="xs:string" default="enable ASF3.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="0"/>
                          <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                          <xs:attribute name="length" type="xs:int" default="1"/>
                          <xs:attribute name="conversion" type="xs:string"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_control_var">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="control_var_type">
                          <xs:attribute name="type" default="uint"/>
                          <xs:attribute name="description" type="xs:string" default="enable ASF3.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="control_gain"/>
                          <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                          <xs:attribute name="length" type="xs:int" fixed="1"/>
                          <xs:attribute name="conversion" type="xs:string"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_mode" type="hyst_direction" default="UPWARD"/>
                  <xs:element name="hyst_trigger" type="trigger_pt_couplet_type"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="chromatix_asf30_reserve" type="chromatix_asf30_reserveType"/>
      <xs:element name="chromatix_asf30_core" type="chromatix_asf30_coreType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_asf30_reserveType">
    <xs:sequence>
      <xs:element name="radial_anchor_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="radial_anchor">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="4" fixed="false"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="anchor table (distance percentage)for radius based noise reduction"/>
          <xs:attribute name="default_value" type="xs:string" default="0.4 0.6 0.8 1"/>
          <xs:attribute name="range" type="xs:string" default="[0.0, 1.0]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="float"/>
          <xs:attribute name="length" type="xs:string" default="4"/>
          <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="face_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of face guided skin color based noise reduction"/>
              <xs:attribute name="default_value" type="xs:int" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="if face_enable = 1 and skin_enable = 1, skin color noise reduction is applied on face regions; if face_enable  = 0 and skin_enable=1, skin color noise reduction is applied on both face regions and non-face regions"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="edge_alignment_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable edge alignment"/>
              <xs:attribute name="default_value" type="xs:int" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of 1st layer 7x7 ASF"/>
              <xs:attribute name="default_value" type="xs:int" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_2_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of 2nd layer 13x13 ASF"/>
              <xs:attribute name="default_value" type="xs:int" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="radial_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of radial distance based sharpening"/>
              <xs:attribute name="default_value" type="xs:int" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="contrast_enable">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable bit of contrast based sharpening."/>
              <xs:attribute name="default_value" type="xs:int" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_asf30_coreType">
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="mod_asf30_total_scale_ratio_data" type="mod_asf30_total_scale_ratio_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_asf30_drc_gain_dataType">
    <xs:sequence>
      <xs:element name="drc_gain_trigger" type="trigger_pt_type"/>
      <xs:element name="drc_gain_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_asf30_hdr_aec_data" type="mod_asf30_hdr_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_asf30_hdr_aec_dataType">
    <xs:sequence>
      <xs:element name="hdr_aec_trigger" type="trigger_pt_type_aec_hdr"/>
      <xs:element name="hdr_aec_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_asf30_aec_data" type="mod_asf30_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_asf30_aec_dataType">
    <xs:sequence>
      <xs:element name="aec_trigger" type="trigger_pt_type_aec"/>
      <xs:element name="asf30_rgn_data" type="asf30_rgn_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_asf30_total_scale_ratio_dataType">
    <xs:sequence>
      <xs:element name="total_scale_ratio_trigger" type="trigger_pt_type"/>
      <xs:element name="total_scalor_ratio_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_asf30_drc_gain_data" type="mod_asf30_drc_gain_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="asf30_rgn_dataType">
    <xs:sequence>
      <xs:element name="layer_1_hpf_symmetric_coeff_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_hpf_symmetric_coeff">
              <xs:simpleType>
                <xs:restriction base="intvar">
                  <xs:length value="10"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="int"/>
          <xs:attribute name="description" type="xs:string" default="1st layer 7x7 sharpening coefficients"/>
          <xs:attribute name="default_value" type="xs:string" default="0, 0, 0, -1, -2, -25, -86, -173, -180, 1968"/>
          <xs:attribute name="range" type="xs:string" default="[[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-1023, 1023],[-2047, 2047]]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="12 bits([9], center), 11 bits([8]), 10 bits(otherwise). All are Q10 numbers"/>
          <xs:attribute name="length" type="xs:int" fixed="10"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="must_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="don't interpolate"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_lpf_symmetric_coeff_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_lpf_symmetric_coeff">
              <xs:simpleType>
                <xs:restriction base="intvar">
                  <xs:length value="10"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="int"/>
          <xs:attribute name="description" type="xs:string" default="Description: 1st layer 7x7 low-pass filter coefficients"/>
          <xs:attribute name="default_value" type="xs:string" default="4 8 12 16 16 24 32 36 48 64"/>
          <xs:attribute name="range" type="xs:string" default="[[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-511, 511],[-1023, 1023],[-2047, 2047]]"/>
          <xs:attribute name="bit_depth" type="xs:string" default=" 12 bits([9], center), 11 bits([8]), 10 bits(otherwise). All are Q10 numbers"/>
          <xs:attribute name="length" type="xs:int" fixed="10"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="don't interpolate"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_activity_band_pass_coeff_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_activity_band_pass_coeff">
              <xs:simpleType>
                <xs:restriction base="intvar">
                  <xs:length value="6"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="int"/>
          <xs:attribute name="description" type="xs:string" default="1st layer 5x5 activity band-pass filter (one-eighth coefficients)"/>
          <xs:attribute name="default_value" type="xs:string" default="-4, -16, -24, 0, 32, 112"/>
          <xs:attribute name="range" type="xs:string" default="[[-127, 127],[-127, 127],[-127,127],[-127, 127],[-255, 255],[-511, 511]]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="10 bits([5], center), 9 bits([4]), 8 bits(otherwise). All are Q8 numbers"/>
          <xs:attribute name="length" type="xs:int" fixed="6"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="don't interpolate"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_activity_normalization_lut_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_activity_normalization_lut">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="level-based normalization LUT to calculate normalized activity"/>
          <xs:attribute name="default_value" type="xs:string" default="0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996 "/>
          <xs:attribute name="range" type="xs:string" default="[0.0, 0.99609375]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="8 bits. Unsigned Q8 numbers"/>
          <xs:attribute name="length" type="xs:int" fixed="64"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_weight_modulation_lut_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_weight_modulation_lut">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="normalized activity-based LUT to control the slope(output/input) when sharpened value magnitude is less than the soft threshold"/>
          <xs:attribute name="default_value" type="xs:string" default="0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 0.996 0.996 0.996 0.996 0.996 0.996  0.996 0.996 "/>
          <xs:attribute name="range" type="xs:string" default="[0.0, 0.99609375]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="8 bits. Unsigned Q8 numbers"/>
          <xs:attribute name="length" type="xs:int" fixed="64"/>
          <xs:attribute name="conversion" type="xs:string" default="no conversion needed"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="layer_1_soft_threshold_lut_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="layer_1_soft_threshold_lut">
              <xs:simpleType>
                <xs:restriction base="uintvar">
                  <xs:length value="64"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="level-based soft-thresholding LUT to determine the final sharpened value"/>
          <xs:attribute name="default_value" type="xs:string" default="255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255 255"/>
          <xs:attribute name="range" type="xs:string" default="[0, 255]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="8 bits. Unsigned Q0 numbers"/>
