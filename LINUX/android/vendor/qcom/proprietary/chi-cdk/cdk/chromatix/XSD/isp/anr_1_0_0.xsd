<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:include schemaLocation="ispglobalelements.xsd"/>
    <xs:element name="anr10_ipe" type="chromatix_anr10Type">
        <xs:annotation>
            <xs:documentation>anr10 ipe module parameters</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:complexType name="mod_anr10_pass_reserve_dataType">
        <xs:sequence>
            <xs:element name="pass_trigger">
                <xs:complexType>
                    <xs:simpleContent>
                        <xs:extension base="trigger_pass">
                            <xs:attribute name="default_value" type="xs:string" default="PASS_FULL,PASS_DC4,PASS_DC16,PASS_DC64"/>
                            <xs:attribute name="tool_hint" default="readonly"/>
                        </xs:extension>
                    </xs:simpleContent>
                </xs:complexType>
            </xs:element>
            <xs:element name="pass_data">
                <xs:complexType>
                    <xs:sequence>
                <xs:element name="top">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="enable_luma_noise_reduction_pass">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="Enable Luma filtering for current PASS. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="must_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_chroma_noise_reduction_pass">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="Enable Chroma filtering for current PASS. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="must_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="power_control">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="enable_chroma_filter_extension">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="1 - Standard mode. . 0 - Low power mode, disable chroma filter extension: turn off median, second bilateral and chroma indicaton extention. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_luma_smoothing_treatment_and_peak_treatment">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="if set to 0, luma_smoothing_treatment_and_peak_treatment will be turned off. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_chroma_smoothing_treatment_and_peak_treatment">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="if set to 0, chroma_smoothing_treatment_and_peak_treatment will be turned off. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_luma_chroma_filter_all_thresholds_per_uv">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="if set to 0, y,u,v thresholds will be dependent only on Ylut (function of y) and not on Uvlut (function of u,v). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_luma_chroma_filter_uv_thresholds_per_yuv">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="if set to 0, u,v thresholds will be a constant and will not be dependent on y,u,v. Constant will be taken from first cell of lut_per_y. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_luma_filter_uv_thresholds">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="if set to 0, luma filter will be dependent only on y derivatives and not u,v derivativs (no need to compare them). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, never_tune, never_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="luma_filter_config">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="filter_decision_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0 - normal mode (depends on iisotropic_min, enable_external). 1 - manual settings ( manual mode, config FiltDerFlags register as user like. (can be configured to disconnect the link betwee 5 depnds on 3 for example ... )). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="filter_enable_external_derivatives">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="is consider dx derivatives for y filtering. 0: don't use. 1:use. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="dcind_decision_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0 - normal mode. 1 - manual settings ( manual mode, config dcindDerFlags register as user like ). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="dcind_enable_external_derivatives">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="is consider dx derivatives for y dc indications. 0: don't use. 1:use. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="enable_use_second_derivative_for_luma_3x3">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="use 2nd derivative for 3x3 comparison decision for luma filtering and dcind and preform OR with regular derivative comparison of regular derivatives. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="chroma_filter_config">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="filter_decision_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0 - normal mode. 1 - manual settings ( manual mode, config FiltDerFlags register as user like. (can be configured to disconnect the link betwee 5 depnds on 3 for example ... )). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="dcind_decision_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0 - normal mode. 1 - manual settings ( manual mode, config dcindDerFlags register as user like ). "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="luma_filter_kernel">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="kernel_definition_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0 - use abstraction (auto config) for deciding on kernels based on granularity and filter size. 1- use manual configured kernels by the user inside the kernels struct. User MUST set edge kernel, flat weight and flat kernel if weight is greater than zero. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="1"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune, rarely_tune, rarely_tune, rarely_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="manual_edge_kernel_complement_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0: dont include pixels in between main directions, 1 include them only if all directions agree. 2- include them only if the two relevant directions (the direction the pixel is in between) agree. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="2"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 2]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="2u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="manual_flat_kernel_complement_mode">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="0: dont include pixels in between main directions, 1 include them only if all directions agree. 2- include them only if the two relevant directions (the direction the pixel is in between) agree. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="2"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 2]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="2u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="luma_peak_management">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="enable_luma_peak_management">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="enable luma peak treatment. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
                                            <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune, often_tune, rarely_tune, never_tune"/>
                                            <xs:attribute name="additional_notes" type="xs:string" default="In tuning_parameter_level, first parmater is for FULL pass, second for DC4, third for DC16 and last is for DC64. Constant (per pass): no dynamic configuration even when control variable change."/>
                                        </xs:extension>
                                    </xs:simpleContent>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="chroma_peak_management">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="enable_chroma_peak_management">
                                <xs:complexType>
                                    <xs:simpleContent>
                                        <xs:extension base="xs:unsignedInt">
                                            <xs:attribute name="type" default="uint"/>
                                            <xs:attribute name="description" type="xs:string" default="enable chroma peak treatment. "/>
                                            <xs:attribute name="default_value" type="xs:string" default="0"/>
                                            <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                                            <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                                            <xs:attribute name="length" type="xs:int" fixed="1"/>
                                            <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
