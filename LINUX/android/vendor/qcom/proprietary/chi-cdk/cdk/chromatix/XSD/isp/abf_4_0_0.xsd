<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:include schemaLocation="ispglobalelements.xsd"/>
  <xs:element name="abf40_bps" type="chromatix_abf40Type">
    <xs:annotation>
      <xs:documentation>abf40 bps module parameters</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="chromatix_abf40Type">
    <xs:sequence>
      <xs:element name="enable_section">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="bilateral_en" default="1">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="enable bilateral filtering, tier 1 enable"/>
                    <xs:attribute name="default_value" type="xs:string" default="1"/>
                    <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                    <xs:attribute name="length" type="xs:int" default="1"/>
                    <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="minmax_en" default="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="enable built-in min-max pixel filter, tier  1 enable"/>
                    <xs:attribute name="default_value" type="xs:string" default="0"/>
                    <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                    <xs:attribute name="length" type="xs:int" default="1"/>
                    <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="if BPC module is on, this feature is likely off"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
            <xs:element name="dirsmth_en" default="0">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="enable_flag_type">
                    <xs:attribute name="type" default="int"/>
                    <xs:attribute name="description" type="xs:string" default="enable Directional Smoothing filter, tier 1 enable"/>
                    <xs:attribute name="default_value" type="xs:string" default="0"/>
                    <xs:attribute name="range" type="xs:string" default="[0,1]"/>
                    <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                    <xs:attribute name="length" type="xs:int" default="1"/>
                    <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
                    <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                    <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="module_version">
        <xs:complexType>
          <xs:attribute name="major_revision" type="xs:int" default="4"/>
          <xs:attribute name="minor_revision" type="xs:int" default="0"/>
          <xs:attribute name="incr_revision" type="xs:int" default="0"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_variables">
        <xs:complexType>
          <xs:sequence minOccurs="3" maxOccurs="3">
            <xs:element name="control_var_type">
              <xs:simpleType>
                <xs:restriction base="control_var_type">
                  <xs:pattern value="control_drc_gain"/>
                  <xs:pattern value="control_exp_time_ratio"/>
                  <xs:pattern value="control_aec_exp_sensitivity_ratio"/>
                  <xs:pattern value="control_lux_idx"/>
                  <xs:pattern value="control_gain"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="default_value" type="xs:string" default="control_drc_gain, control_aec_exp_sensitivity_ratio, control_gain"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="control_method">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="aec_exp_control" type="tuning_control_aec_type" default="control_gain"/>
            <xs:element name="aec_hdr_control" type="tuning_control_aec_hdr_type" default="control_aec_exp_sensitivity_ratio"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="dynamic_enable_triggers">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="bilateral_en">
              <xs:complexType>
                <xs:sequence>
                  <xs:element name="enable">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="enable_flag_type">
                          <xs:attribute name="type" default="int"/>
                          <xs:attribute name="description" type="xs:string" default="enable ABF4.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="0"/>
                          <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                          <xs:attribute name="length" type="xs:int" default="1"/>
                          <xs:attribute name="conversion" type="xs:string"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_control_var">
                    <xs:complexType>
                      <xs:simpleContent>
                        <xs:extension base="control_var_type">
                          <xs:attribute name="type" default="uint"/>
                          <xs:attribute name="description" type="xs:string" default="enable ABF4.0 module"/>
                          <xs:attribute name="default_value" type="xs:string" default="control_gain"/>
                          <xs:attribute name="range" type="xs:string" default="[0, 1]"/>
                          <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
                          <xs:attribute name="length" type="xs:int" fixed="1"/>
                          <xs:attribute name="conversion" type="xs:string"/>
                          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
                          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
                        </xs:extension>
                      </xs:simpleContent>
                    </xs:complexType>
                  </xs:element>
                  <xs:element name="hyst_mode" type="hyst_direction" default="UPWARD"/>
                  <xs:element name="hyst_trigger" type="trigger_pt_couplet_type"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="chromatix_abf40_reserve" type="chromatix_abf40_reserveType"/>
      <xs:element name="chromatix_abf40_core" type="chromatix_abf40_coreType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_abf40_reserveType">
    <xs:sequence>
      <xs:element name="cross_plane_en">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable Gr/Gb cross processing in bilateral filtering"/>
              <xs:attribute name="default_value" type="xs:string" default="1"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" default="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="dark_desat_en" default="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable dark desaturation feature, tier 2 enable"/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" default="1"/>
              <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="Effective when bilateral_en is on."/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="dark_smooth_en" default="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable dark area smoothing feature, tier 2 enable"/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" default="1"/>
              <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="Effective when bilateral_en is on."/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="act_adj_en" default="0">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="enable_flag_type">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="enable activity based adjustment feature, tier 2 enable"/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0,1]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="1u"/>
              <xs:attribute name="length" type="xs:int" default="1"/>
              <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="It is effective only when both bilateral_en and dirsmth_en are set to 1"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="blk_opt">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:int">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="block matching pattern for R/B "/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0, 2]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="2u"/>
              <xs:attribute name="length" type="xs:int" default="1"/>
              <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="rarely_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="radial_anchor_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="radial_anchor">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="5"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="anchor tables from center to frame boundary"/>
          <xs:attribute name="default_value" type="xs:string" default="0,0.4,0.6,0.8,1.0"/>
          <xs:attribute name="range" type="xs:string" default="[0.0, 1.0]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="float"/>
          <xs:attribute name="length" type="xs:int" fixed="5"/>
          <xs:attribute name="conversion" type="xs:string" default="No Conversion needed "/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="must_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="rnr_anchor[0] is 0 by default. normally rnr_anchor[4] is 1.0."/>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="chromatix_abf40_coreType">
    <xs:sequence maxOccurs="unbounded">
      <xs:element name="mod_abf40_drc_gain_data" type="mod_abf40_drc_gain_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_abf40_drc_gain_dataType">
    <xs:sequence>
      <xs:element name="drc_gain_trigger" type="trigger_pt_type"/>
      <xs:element name="drc_gain_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_abf40_hdr_aec_data" type="mod_abf40_hdr_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_abf40_hdr_aec_dataType">
    <xs:sequence>
      <xs:element name="hdr_aec_trigger" type="trigger_pt_type_aec_hdr"/>
      <xs:element name="hdr_aec_data">
        <xs:complexType>
          <xs:sequence maxOccurs="unbounded">
            <xs:element name="mod_abf40_aec_data" type="mod_abf40_aec_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_abf40_aec_dataType">
    <xs:sequence>
      <xs:element name="aec_trigger" type="trigger_pt_type_aec"/>
      <xs:element name="abf40_rgn_data" type="abf40_rgn_dataType"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="abf40_rgn_dataType">
    <xs:sequence>
      <xs:element name="minmax_maxshft">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:unsignedInt">
              <xs:attribute name="type" default="uint"/>
              <xs:attribute name="description" type="xs:string" default="upper/lower offset (as right-shifting) used in single bpc applied to neighbor pixels"/>
              <xs:attribute name="default_value" type="xs:string" default="3"/>
              <xs:attribute name="range" type="xs:string" default="[0, 15]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="4u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="minmax_minshft">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:unsignedInt">
              <xs:attribute name="type" default="uint"/>
              <xs:attribute name="description" type="xs:string" default="upper/lower offset (as right-shifting) used in single bpc applied to neighbor pixels"/>
              <xs:attribute name="default_value" type="xs:string" default="3"/>
              <xs:attribute name="range" type="xs:string" default="[0, 15]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="4u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="minmax_offset">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:unsignedInt">
              <xs:attribute name="type" default="uint"/>
              <xs:attribute name="description" type="xs:string" default="common absolute offset value used in single bpc applied to both center pixel and neighboring pixel"/>
              <xs:attribute name="default_value" type="xs:string" default="64"/>
              <xs:attribute name="range" type="xs:string" default="[0, 4095]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="12u"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string" default="No Conversion needed"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="Q0 precision"/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="minmax_bls">
        <xs:complexType>
          <xs:simpleContent>
            <xs:extension base="xs:int">
              <xs:attribute name="type" default="int"/>
              <xs:attribute name="description" type="xs:string" default="black level used in single bpc applied to both center pixel and neighboring pixel"/>
              <xs:attribute name="default_value" type="xs:string" default="0"/>
              <xs:attribute name="range" type="xs:string" default="[0, 4095]"/>
              <xs:attribute name="bit_depth" type="xs:string" default="12 bit unsigned"/>
              <xs:attribute name="length" type="xs:int" fixed="1"/>
              <xs:attribute name="conversion" type="xs:string"/>
              <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
              <xs:attribute name="additional_notes" type="xs:string" default="none"/>
              <xs:attribute name="tool_hint" default="float"/>
            </xs:extension>
          </xs:simpleContent>
        </xs:complexType>
      </xs:element>
      <xs:element name="blkpix_lev_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="blkpix_lev">
              <xs:simpleType>
                <xs:restriction base="uintvar">
                  <xs:length value="2"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="block matching or pixel matching selection. Value 0 means block matching in determining filter coefficient. Value 4 means pixel matching"/>
          <xs:attribute name="default_value" type="xs:string" default="0 0"/>
          <xs:attribute name="range" type="xs:string" default="[0, 4]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="3u"/>
          <xs:attribute name="length" type="xs:int" fixed="2"/>
          <xs:attribute name="conversion" type="xs:string"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="noise_std_lut_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="noise_std_lut">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="65"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="float"/>
          <xs:attribute name="description" type="xs:string" default="noise profile (std) that will be used together with edge_softness to generate noise_std_dev_lut in packed format"/>
          <xs:attribute name="default_value" type="xs:string" default="0 6 9 11 12 14 15 16 18 19 20 21 22 23 24 24 25 26 27 27 28 29 30 30 31 32 32 33 33 34 35 35 36 36 37 37 38 39 39 40 40 41 41 42 42 43 43 44 44 44 44 44 44 44 44 44 44 44 44 44 44 44 44 44 44"/>
          <xs:attribute name="range" type="xs:string" default="[0.0, 512.0]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="float"/>
          <xs:attribute name="length" type="xs:int" fixed="65"/>
          <xs:attribute name="conversion" type="xs:string"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="must_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="The above conversion starts with 8996 ABF noise profile"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="curve_offset_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="curve_offset">
              <xs:simpleType>
                <xs:restriction base="uintvar">
                  <xs:length value="4"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="offset that is subtracted from normalized difference before generating bilateral coefficients"/>
          <xs:attribute name="default_value" type="xs:string" default="0 0 0 0"/>
          <xs:attribute name="range" type="xs:string" default="[0, 127]"/>
          <xs:attribute name="bit_depth" type="xs:string" default=" 7u"/>
          <xs:attribute name="length" type="xs:int" fixed="4"/>
          <xs:attribute name="conversion" type="xs:string"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="none"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="dist_ker_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="dist_ker">
              <xs:simpleType>
                <xs:restriction base="intvar">
                  <xs:length value="18"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
          <xs:attribute name="type" default="uint"/>
          <xs:attribute name="description" type="xs:string" default="bit shifting value corresponding to distance weight; [0-2][] -- 3 smoothing levels, [][0-2,R/B, 3-5,Gr/Gb] non-zero value n means 1-shift-left-(n-1); zero value means weight of 0."/>
          <xs:attribute name="default_value" type="xs:string" default="3 2 1 3 2 1 3 2 1 3 2 1 3 2 1 3 2 1"/>
          <xs:attribute name="range" type="xs:string" default="[[1,4],[0,2],[0,1],[1,4],[0,2],[0,1],[1,4],[0,2],[0,1],[1,4],[0,2],[0,1],[1,4],[0,2],[0,1],[1,4],[0,2],[0,1]]"/>
          <xs:attribute name="bit_depth" type="xs:string" default="[][0]:3bit; othes:2bit"/>
          <xs:attribute name="length" type="xs:string" fixed="3x6"/>
          <xs:attribute name="conversion" type="xs:string"/>
          <xs:attribute name="tuning_parameter_level" type="tuning_level" default="often_tune"/>
          <xs:attribute name="additional_notes" type="xs:string" default="range:[0]:1-4; [1]:0-2; [2]:0-2"/>
          <xs:attribute name="tool_hint" default="float"/>
        </xs:complexType>
      </xs:element>
      <xs:element name="edge_softness_tab">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="edge_softness">
              <xs:simpleType>
                <xs:restriction base="floatvar">
                  <xs:length value="4"/>
                </xs:restriction>
              </xs:simpleType>
            </xs:element>
          </xs:sequence>
