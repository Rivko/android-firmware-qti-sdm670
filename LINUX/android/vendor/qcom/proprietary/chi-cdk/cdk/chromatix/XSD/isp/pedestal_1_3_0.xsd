          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_pedestal13_cct_dataType">
    <xs:sequence>
      <xs:element name="cct_trigger" type="trigger_pt_type"/>
