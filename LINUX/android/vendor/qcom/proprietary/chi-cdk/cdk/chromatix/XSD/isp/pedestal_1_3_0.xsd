            <xs:element name="mod_pedestal13_led_idx_data" type="mod_pedestal13_led_idx_dataType"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="mod_pedestal13_led_idx_dataType">
    <xs:sequence>
      <xs:element name="led_idx_trigger" type="trigger_pt_type_led_idx"/>
