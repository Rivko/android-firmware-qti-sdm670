////////////////////////////////////////////////////////////
///
///  GET_ELF_BUILDROOT
///  @brief Finds buildroot given elf path and root name
///
////////////////////////////////////////////////////////////
GET_ELF_BUILDROOT:
            LOCAL &local_elffile &local_buildroot &processor_root_name
            ENTRY &local_elffile &processor_root_name
            
            IF ("&processor_root_name"=="")||("&processor_root_name"=="NULL")
            (
                GOSUB GET_BUILD_PROC_ROOT_NAME &local_elffile
                ENTRY &processor_root_name
                IF "&processor_root_name"=="NULL"
                (
                    RETURN &processor_root_name
                )
            )
            
            LOCAL &location &what_to_cut &filepath
            &location=STRING.SCAN("&local_elffile","&processor_root_name",0)
            &what_to_cut=&location-STRING.LENGTH("&local_elffile")-1
            &filepath=STRING.CUT("&local_elffile",&what_to_cut)

            //If not a legitimate  path, return NULL
            IF (OS.DIR("&filepath")&&("&filepath"!=""))
            (
                
                &local_buildroot=OS.FILE.PATH(&filepath/&processor_root_name)
                GOSUB SANITIZEPATHS &local_buildroot
                ENTRY &local_buildroot
            )
            ELSE
            (
                &local_buildroot="NULL"
            )
            
            
            RETURN &local_buildroot
            
////////////////////////////////////////////////////////////
///
///  PARSEOUTLIST
///  @brief Parses out a given comma-separated list. Returns first element and remainder of list and a flag
///         Meant to be iterated over from caller.
///
///  arguments: deliminator: symbol to deliminate around (usually a ','). Can be anything but space
///             givenlist: comma-separated list of inputs. If space separated, then spaces are replaced by the deliminator argument
///
///  returns:   newlist - list with first element removed.
///             return_value - first element of list returned
///             flag - TRUE - inidcates if list is done
///                    FALSE - indicates list can continue.
////////////////////////////////////////////////////////////
//An example caller of this script can be found in  SANITIZEPATHS       
PARSEOUTLIST:
        LOCAL &deliminator &newlist &return_value &flag &givenstring &rvalue
        ENTRY &deliminator %LINE &givenstring
        
        &givenstring=STRING.TRIM("&givenstring")
        IF STRING.SCAN("&givenstring"," ",0)!=-1
        (
            &givenstring=STRING.REPLACE("&givenstring"," ","&deliminator",0)
        )
        
        LOCAL &count &maxcount &return_value &size_of_returnvalue &string_length &string_size &value_to_cut
        
        &current_string="&givenstring"
        
        &string_length=String.Length("&current_string")
        &string_size=string.scan("&current_string","&deliminator",0)
        
        IF (&string_size!=-1)
        (
            &value_to_cut=&string_size-&string_length    
            //separate the first master from the rest of the string
            &return_value=string.cut("&current_string",&value_to_cut)
            &size_of_returnvalue=STRING.LENGTH("&return_value")
            &newlist=STRING.CUT("&current_string",&size_of_returnvalue+1)
            &flag="FALSE"
        )
        ELSE //if no "&deliminator" found, exit condition
        (
            &return_value="&current_string"
            &newlist="NULL"
            &flag="TRUE"
            &count=&maxcount
        )
        
        &count=&count+1
    
        
        
    RETURN &return_value &newlist &flag

    
/////////////////////////////////////////////
///
/// EVALUATE_INTERCOM_ATTACH_STATE
/// @brief Retrieves attachstate from specified t32 window.
/// Arguments:  targetprocessorport - numerical value - intercom port number for desired window.
/// Return values:
///         ATTACHED - T32 window is attached to desired target
///         PREPARED - T32 window is in sys.m.prepare state
///         FALSE    - T32 window is not attached
///         CLOSED   - T32 window is not open.
///
/////////////////////////////////////////////
EVALUATE_INTERCOM_ATTACH_STATE:
    LOCAL &targetprocessorport
    ENTRY &targetprocessorport
    
    LOCAL &rvalue
    LOCAL &localhost
    &localhost=INTERCOM.PORT()
    IF "&targetprocessorport"=="&localhost"
    (
        IF SYSTEM.MODE()==0xB
        (
            &rvalue="ATTACHED"
        )
        ELSE IF SYSTEM.MODE()==0x4
        (
            &rvalue="PREPARED"
        )
        ELSE
        (
            &rvalue="FALSE"
        )
    )
    ELSE
    (
        IF (INTERCOM.PING(&targetprocessorport))
        (
            INTERCOM.EVALUATE &targetprocessorport SYSTEM.MODE()
            LOCAL &attachstate
            &attachstate=EVAL()
            IF ("&attachstate"=="0x0B")
            (
                &rvalue="ATTACHED"
            )
            ELSE IF ("&attachstate"=="0x04")
            (
                &rvalue="PREPARED"
            )
            ELSE
            (
                &rvalue="FALSE"
            )
        )
        ELSE
        (
            &rvalue="CLOSED"
        )
    )
    
    
    RETURN &rvalue
    

/////////////////////////////////////////////
///
/// EVALUATE_INTERCOM_STOP_STATE
/// @brief Retrieves attachstate from specified t32 window.
/// Arguments:  targetprocessorport - numerical value - intercom port number for desired window.
/// Return values:
///         STOPPED - T32 window is attached and processor is in debug halt state.
///         RUNNING - T32 window is attached to processor, processor is in running state.
///         PREPARED - T32 window is in sys.m.prepare state
///         FALSE    - T32 window is not attached
///         CLOSED   - T32 window is not open.
///
/////////////////////////////////////////////
EVALUATE_INTERCOM_STOP_STATE:
    LOCAL &targetprocessorport
    ENTRY &targetprocessorport
    
    LOCAL &rvalue
    LOCAL &localhost
    &localhost=INTERCOM.PORT()
    IF "&targetprocessorport"=="&localhost"
    (
        IF SYSTEM.MODE()==0xB
        (
            IF FALSE()==STATE.RUN()
            (
                &rvalue="STOPPED"
            )
            ELSE
            (
                &rvalue="RUNNING"
            )
        )
        ELSE IF SYSTEM.MODE()==0x4
        (
            &rvalue="PREPARED"
        )
        ELSE
        (
            &rvalue="FALSE"
        )
    )
    ELSE
    (
        IF (INTERCOM.PING(&targetprocessorport)
        (
            INTERCOM.EVALUATE &targetprocessorport SYSTEM.MODE()
            LOCAL &attachstate
            IF ("&attachstate"=="0x0B")
            (
                INTERCOM.EVALUATE &targetprocessorport STATE.RUN()
                LOCAL &stopstate
                &stopstate=EVAL()
                IF (1==&stopstate)
                (
                    &rvalue="RUNNING"
                )
                ELSE
                (
                    &rvalue="STOPPED"
                )
            )
            ELSE IF ("&attachstate"=="0x04")
            (
                &rvalue="PREPARED"
            )
            ELSE
            (
                &rvalue="FALSE"
            )
        )
        ELSE
        (
            &rvalue="CLOSED"
        )
    )
    
    
    RETURN &rvalue
/////////////////////////////////////////////
///
/// WAIT_TO_STOP
/// @brief Waits for processor to stop running.
/// Arguments:  breakpoint - only used for error message
///             countvalue_ms - value in milliseconds. If &RUMI=="TRUE", it's value in ms*100
///             targetprocessorport - port value. If not given, will assume local processor window.
///             scriptname - not necessary, but error message will print scriptname as calling script
///
/////////////////////////////////////////////
WAIT_TO_STOP:
    LOCAL &breakpoint &countvalue_ms &targetprocessorport
    ENTRY &breakpoint &countvalue_ms &targetprocessorport %LINE &therest

    IF ("&scriptname"=="NULL")||("&scriptname"=="")
    (
        "&scriptname"="Script Error"
    )
    IF ("&targetprocessorport"=="NULL")||("&targetprocessorport"=="")
    (
        &targetprocessorport=INTERCOM.PORT()
    )
    IF ("&countvalue_ms"=="")
    (
        do std_fatalexit SYSTEM "Error - invalid countvalue_ms given for WAIT_TO_STOP routine. countvalue_ms: &countvalue_ms"
        plist
        pstep
    )
    &countvalue_ms=(&countvalue_ms/100)

    
    
    LOCAL &T32_runstate &count
    &count=0
    &retries=0
    &maxretries=3
    INTERCOM.EVALUATE &targetprocessorport STATE.RUN()
    &T32_runstate=EVAL()
    INTERCOM.EVALUATE &targetprocessorport SYSTEM.MODE()
    &T32_attachstate=EVAL()
    
    wait.100ms
    
    WHILE ((&T32_runstate==1)&&(&count<&countvalue_ms)&&(&retries<&maxretries))
    (
        INTERCOM.EVALUATE &targetprocessorport STATE.RUN()
        &T32_runstate=EVAL()
        &count=&count+1
        IF (&T32_runstate==0)
        (
            &retries=&retries+1
        )
        IF ("&RUMI"=="TRUE")
        (
            Wait.2s
        )
        ELSE
        (
            wait.100ms
        )
    )
    IF ("&targetprocessorport"!="&LOCALHOST")
    (
        INTERCOM.WAIT &targetprocessorport
    )
    INTERCOM.EVALUATE &targetprocessorport STATE.RUN()
    &T32_runstate=EVAL()
    IF (1==&T32_runstate)
    (
        //Try one more time.
        wait.1s
        INTERCOM.EVALUATE &targetprocessorport STATE.RUN()
        &T32_runstate=EVAL()
        IF (1==&T32_runstate)
        (
            
            GOSUB REMOTE_PROC_NOT_STOPPED &breakpoint
            &USEEXISTINGSYMS_TARGET="FALSE"
            &ENTRYHEXADDR_TARGET="NULL"
            LOCAL &breakpointexitstring
            IF "&breakpoint"=="NULL"
            (
                &breakpointexitstring="!"
            )
            ELSE
            (
                &breakpointexitstring=" at &breakpoint."
            )   
            &exitstring=PRACTICE.CALLER.FILE(1.)+" - symbol mismatch; processor didn't halt&breakpointexitstring"
            
            do std_fatalexit USER &exitstring
        )
    )
    
    RETURN SUCCESS    
    
REMOTE_PROC_NOT_STOPPED:
    LOCAL &argline
    ENTRY %LINE &argline
    
        WINPOS 0. 0. 50. 10.
        area.reset
        area
        PRINT %ERROR "   Was not able to stop remote processor. "
        PRINT        "   Breakpoint: &argline "
        PRINT %ERROR "   Please Check if:"
        PRINT %ERROR "       - Symbols mismatch,"
        PRINT %ERROR "       - Need to restart remote T32 window"
        PRINT %ERROR "       - Apps timeout resulting "
        PRINT " "
        PRINT " "
        PRINT " "
        //Don't use same symbols on next run
        
        RETURN 
        
/////////////////////////////////////////
///
/// @brief Because PRACTICE doesn't give us file search utilities, 
/// recursively search given serachpath directory for file.
///
/////////////////////////////////////////
GET_SCRIPT_PATH:

            LOCAL &rvalue &origdir
            LOCAL &ACTION &searchpath &script_to_look_for
            ENTRY &ACTION &searchpath &script_to_look_for
            
            IF !OS.DIR("&searchpath")||("&searchpath"=="")
            (
                RETURN "NULL"
            )
            
            &searchpath=STRING.REPLACE("&searchpath","\","/",0)
            GOSUB REMOVESLASH exit &searchpath
            ENTRY &notused &searchpath %LINE &therest
            // Load the memory map again for relocated images
            //do std_memorymap DYNAMIC
            ON ERROR CONTINUE

            &tempfile="&TEMPDIR\listfiles.txt"
            PRINTER.FILE &tempfile
            PRINTER.FILETYPE ASCII
            PRINTER.SIZE 0XFA, 0XFA
            
            &origdir=OS.PWD()
            CD &searchpath
            //WINPOS 0. 100% 10. 0. 
            WP.DIR
            
            
            LOCAL &linelst 
            &linelst="NULL"
            open #1 &tempfile
            LOCAL &directoryfound &scriptmatch
            &directoryfound=""
            &scriptmatch=0
            
            WHILE (("&linelst"!="")&&(&scriptmatch==0.))
            (
                read #1 %line &linelst
                IF !(("&linelst"=="B::DIR")||("&linelst"==".")||("&linelst"=="..")||("&directoryfound"!=""))
                (
                    &directoryfound="&linelst"
                )
                
                IF ("&script_to_look_for"=="&linelst")
                (
                    &scriptmatch=1
                )
                
            )
            close #1
            ON ERROR
            
            IF (&scriptmatch==1)
            (
                &rvalue="&searchpath"
                CD &origdir
            )
            ELSE IF (OS.DIR("&searchpath/&directoryfound")&&("&directoryfound"!=""))
            (
                do std_utils GET_SCRIPT_PATH EXIT &searchpath/&directoryfound
                LOCAL &returnpath
                ENTRY &returnpath
                IF "&returnpath"=="&searchpath/&directoryfound"
                (
                    &rvalue="&returnpath"
                )
            )
            ELSE
            (
                &rvalue="NULL"
            )
                
            print " "
            
            RETURN &rvalue
    
  
EXIT:
    LOCAL &rvalue
    ENTRY %LINE &rvalue
    ENDDO &rvalue
    


FATALEXIT:
    LOCAL &rvalue
    ENTRY %LINE &rvalue
    IF STRING.SCAN("&FAILUREKEYWORD","FAILUREKEYWORD",0)==-1
    (
        GOSUB EXIT &FAILUREKEYWORD - &rvalue
    )
    ELSE
    (
        GOSUB EXIT &rvalue
    )

    
//Should never get here
    END

//Prints log file and exits scripts
DOFATALEXIT:
    LOCAL &rvalue
    ENTRY %LINE &rvalue

    //No return from here
    do std_fatalexit &rvalue
    
//Should never get here
    END
    
