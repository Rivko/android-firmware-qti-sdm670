////////////////////////////////////////////////////
///
/// FindSymbol
/// @brief Finds a symbol. Note that &FindSymbolResult must be declared outside this context
///        Meant to be called via symbol.foreach from calling function.
///
/////////////////////////////////////////////////////    
FINDSYMBOL:
    
    LOCAL &symbol &searchstring &searchstring2
    ENTRY &symbol &searchstring &searchstring2
    //Assumes &FindSymbolResult has been declared above
    &symbol=SYMBOL.NAME(&symbol)

    IF "&searchstring2"==""
    (    
        IF (STRING.SCAN("&symbol","&searchstring",0)!=-1)
        (
            IF (STRING.SCAN("&FindSymbolResult","&symbol",0)==-1)
            (
                IF ("&FindSymbolResult"=="")
                (
                    &FindSymbolResult=SYMBOL.NAME(&symbol)
                )
                ELSE
                (
                    &FindSymbolResult="&FindSymbolResult"+","+SYMBOL.NAME(&symbol)
                )
            )
        )
    )
    ELSE
    (    
        IF (STRING.SCAN("&symbol","&searchstring",0)!=-1)&&(STRING.SCAN("&symbol","&searchstring2",0)!=-1)
        (
            IF (STRING.SCAN("&FindSymbolResult","&symbol",0)==-1)
            (
                IF ("&FindSymbolResult"=="")
                (
                    &FindSymbolResult=SYMBOL.NAME(&symbol)
                )
                ELSE
                (
                    &FindSymbolResult="&FindSymbolResult"+","+SYMBOL.NAME(&symbol)
                )
            )
        )
    )
    
    RETURN

/////////////////////////////////////
///
///    Function: SANITIZEPATHS
///    @brief Replaces '\' with '/' for use in structures. 
///           If path is not valid, replaces it with 'NULL'
/////////////////////////////////////
SANITIZEPATHS:
    LOCAL &argline
    ENTRY %LINE &argline
    LOCAL &currentpathtext &listdoneflag &rvalue &currpath
    &currentpathtext="&argline"
    &listdoneflag="FALSE"
    WHILE ("&listdoneflag"=="FALSE")
    (
        GOSUB PARSEOUTLIST , &currentpathtext
        ENTRY &currpath &currentpathtext &listdoneflag
        
        IF OS.FILE("&currpath")||OS.DIR("&currpath")
        (
            &currpath=STRING.REPLACE("&currpath","\","/",0)
            &rvalue="&rvalue &currpath"
        )
        ELSE
        (
            &rvalue="&rvalue NULL"
        )
    )
    &rvalue=STRING.TRIM("&rvalue")
RETURN &rvalue

////////////////////////////////////////////////////////////
///
///  LOAD_ELF_UTILITY
///  @brief Generic loader for elf files. 
///
////////////////////////////////////////////////////////////
LOAD_ELF_UTILITY:
    LOCAL &elffile &buildroot &proc_name &offset &elfoptions
    ENTRY &elffile &buildroot &proc_name &offset %LINE &elfoptions
    LOCAL &rvalue
    &rvalue="SUCCESS"
    
    IF !OS.FILE("&elffile")
    (
        RETURN FAILURE
    )
    LOCAL &path_set_usable
    IF ("&buildroot"=="NULL")||("&buildroot"=="")||(!OS.DIR("&buildroot"))
    (
        GOSUB GET_ELF_BUILDROOT &elffile &proc_name
        ENTRY &buildroot

    )
    ELSE IF ("&proc_name"=="NULL")||("&proc_name"=="")&&(STRING.SCAN("&buildroot","&proc_name",0)!=-1)
    (
        GOSUB GET_BUILD_PROC_ROOT_NAME &elffile
        ENTRY &proc_name
    )
    
    IF ("&buildroot"=="NULL")||("&proc_name"=="NULL")
    (
        &path_set_usable="FALSE"
    )
    ELSE
    (
        &path_set_usable="TRUE"
    )
    
    //Format given elf options
    &elfoptions=STRING.TRIM("&elfoptions")
    //Remove unwanted/unneeded options
    &elfoptions=STRING.REPLACE("&elfoptions","strippart","",0)
    &elfoptions=STRING.REPLACE("&elfoptions","sourcepath","",0)
    &elfoptions=STRING.REPLACE("&elfoptions"," ",",",0)
    &elfoptions=STRING.REPLACE("&elfoptions",","," /",0)
    IF "&elfoptions"!=""
    (
        &elfoptions="/"+"&elfoptions"
    )
    &offset=STR.UPR("&offset")
    IF ("&offset"=="NULL")||("&offset"=="")
    (
        &offset=""
    )
    ELSE
    (
        IF (STRING.SCAN("&offset","0X",0)==-1)
        (
            PRINT "Error! LOAD_ELF_UTILITY: Invalid offset given (not null, blank or hex address: &offset. Returning"
            RETURN FAILURE
        )
    )
    
    ON ERROR GOSUB 
    (
        ON ERROR DEFAULT
        PRINT "Error ocurred during elf load. Elf file: &elffile"
        RETURN FAILURE
    )
    IF "&path_set_usable"=="TRUE"
    (
        D.LOAD.ELF &elffile &offset &elfoptions /strippart "&proc_name" /sourcepath "&buildroot"
    )
    ELSE
    (
        D.LOAD.ELF &elffile &offset &elfoptions
    )
    
    ON ERROR DEFAULT
    RETURN &rvalue



