&image="slpi"

    do std_debug_&CHIPSET GETDEBUGDEFAULTS NONE &image &lpm_option &cti_enable &alternateelf &extraoption
        ENTRY &image &targetprocessor &targetprocessorport &bootprocessor &bootprocessorport &peripheral_processor &processortimeoutvalue &debugscript &symbolloadscript &imagebuildroot &entry_bkpt &error_bkpts &lpm_option &sleep_disable_command &cti_enable &multi_elf_option &alternateelf &extraoption

        
//#####################Select Subroutine###################
// Name of the utility we are calling
LOCAL &SUBROUTINE
// Any subroutine specific options
// Default exists for each subroutine
LOCAL &OPTION

// Input Argument 0 is the name of the utility
&SUBROUTINE="&UTILITY"
IF !(("&SUBROUTINE"=="WAKEPROCESSOR")||("&SUBROUTINE"=="HALTPROCESSOR")||("&SUBROUTINE"=="WARMBOOTWAKE")||("&SUBROUTINE"=="CRASHPROCESSOR")||("&SUBROUTINE"=="SETCOREASCTIMASTER")||("&SUBROUTINE"=="SENDCTIBREAKTOALLCORES")||("&SUBROUTINE"=="SENDCTIGOTOALLCORES")||("&SUBROUTINE"=="CTI_GUI")||("&SUBROUTINE"=="HELP")||("&SUBROUTINE"=="help"))
(
    &SUBROUTINE="HELP"
    PRINT %ERROR "std_cti_&image: Subroutine not found! Printing Help Window"
)

    // This should be created by some top level script. The setupenv for each proc would
    // set this up
    AREA.SELECT
    // Call the required utility
    GOSUB &SUBROUTINE &OPTION &IARG0 &IARG1 &IARG2 &IARG3 &IARG4 &IARG5 &IARG6 &IARG7 &IARG8 &IARG9 &IARG10 &IARG11
    LOCAL &rvalue
    ENTRY %LINE &rvalue
    IF "&rvalue"==""
    (
        &rvalue="NONE"
    )
    do std_results std_cti_slpi &rvalue "&SUBROUTINE"
    GOTO EXIT
    
////////////////////////////////////////
///
/// @brief Checks if AOP and APPS0 are present
///
////////////////////////////////////////
CHECK_NEEDED_T32_WINDOWS:
        //Check if boot processor window is open. For case of Apps0, can be apps0 or appsc0
    LOCAL &Rvalue
    do std_intercom_init CHECKSESSION 1 &bootprocessor
    ENTRY &Rvalue
    //Special case for apps window - can have apps0 or cluster
    IF (&Rvalue!=1.)&&("&bootprocessor"=="APPS0")
    (
        do std_intercom_init CHECKSESSION 1 APPSC0
        ENTRY &Rvalue
    )
    IF (&Rvalue!=1.)
    (
        PRINT %ERROR "&bootprocessor T32 window not open. Please open and try again"
        do std_fatalexit USER "&FAILURE - std_cti_slpi - &bootprocessor T32 window not open. Please open and try again"
    )
    LOCAL &chipset_upr
    &chipset_upr=STR.UPR("&CHIPSET")
    IF ("&chipset_upr"=="SDM845")&&(&Rvalue!=1.)&&("&bootprocessor"=="APPS0")
    (
        //Check if aop processor window is open. For case of Apps0, can be apps0 or appsc0
        LOCAL &Rvalue
        do std_intercom_init CHECKSESSION 1 RPM
        ENTRY &Rvalue
        IF (&Rvalue!=1.)
        (
            PRINT %ERROR "AOP T32 window not open. Please open and try again"
            do std_fatalexit "&FAILURE - std_cti_slpi - AOP T32 window not open. Please open and try again"
        )
    )

    RETURN SUCCESS
////////////////////////////////////////
//
//          WAKEPROCESSOR
//
//          Just sends a CTI pulse to Q6. 
//          Software on target should busywait for 10 seconds
//          Needs the ability to be run from a remote processor for TPIU calibration.
//
///////////////////////////////////////// 
WAKEPROCESSOR:
LOCAL &break &assume_island
ENTRY &break

    WINPOS 75. 0. 40. 20. 0. 0. 
    AREA
    
    GOSUB CHECK_NEEDED_T32_WINDOWS
    
    //attach to bootprocessor and load hwio
    do std_utils MAKE_SURE_BOOT_PROCESSOR_ATTACHED prepare

    //Run HWIO if needed

    do std_intercom_do &targetprocessorport hwio
    
    do std_utils EVALUATE_INTERCOM_ATTACH_STATE &bootprocessorport
    LOCAL &bootattachstate
    ENTRY &bootattachstate
    IF ("&bootattachstate"=="ATTACHED")
    (
        &assume_island="FALSE"
    )
    ELSE
    (
        &assume_island="TRUE"
    )
    
    
    
    do std_utils EVALUATE_INTERCOM_ATTACH_STATE &targetprocessorport
    LOCAL &attachstate
    ENTRY &attachstate
    IF ("&attachstate"!="ATTACHED")
    (
        INTERCOM.EXECUTE &targetprocessorport SYSTEM.MODE.PREPARE
    )
    
    //Only write to IMEM etc. if we believe system is not in power collapse
    IF ("&assume_island"=="FALSE")
    (
        do std_debug_&CHIPSET DISABLE_CLOCK_GATING &image &targetprocessorport
        //disable clock gating
        
        INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&DEBUG_COOKIE_ADDRESS %LONG %LE &DEBUG_COOKIE_VALUE
        INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&INSTRUCTION_COOKIE_ADDRESS %LONG %LE &INSTRUCTION_COOKIE_BUSYWAIT
        
        do std_intercom_do &targetprocessorport std_utils HWIO_OUTF SSC_QDSP6SS_DBG_CFG DBG_SW_REG &dbg_register_value

        wait.200ms    
    )
    GOSUB SEND_CTI_PULSE
    

    do std_utils EVALUATE_INTERCOM_ATTACH_STATE &targetprocessorport
    LOCAL &attachstate
    ENTRY &attachstate
    IF "&EUD"=="TRUE"
    (
        INTERCOM.EXECUTE &targetprocessorport sys.down
        do std_intercom_do &targetprocessorport std_utils ATTACHPROC DONTGO FALSE
    )
    IF ("&attachstate"!="ATTACHED")
    (
        do std_intercom_do &targetprocessorport std_utils ATTACHPROC DONTGO FALSE
    )
    do std_utils EVALUATE_INTERCOM_ATTACH_STATE &targetprocessorport
    LOCAL &attachstate
    ENTRY &attachstate
    IF ("&attachstate"!="ATTACHED")
    (
        RETURN FAILURE
    )
    ELSE
    (
        RETURN SUCCESS
    )
   

////////////////////////////////////////
//
//          HALTPROCESSOR
//
//          Just sends a CTI pulse to Q6. 
//          Software on target should busywait for 10 seconds
//
///////////////////////////////////////// 
HALTPROCESSOR:
LOCAL &break
ENTRY &break

    GOSUB ATTACH_TO_PROCESSOR
    LOCAL &result
    ENTRY &result
    
    do std_utils EVALUATE_INTERCOM_STOP_STATE &targetprocessorport
    LOCAL &stopstate
    ENTRY &stopstate
    
    IF "&stopstate"=="FALSE"
    (
        GOSUB FAILED_TO_BREAK
    )
    ELSE IF "&stopstate"=="RUNNING"
    (
        do std_utils BREAKPROC &targetprocessorport
    )
    
    do std_utils EVALUATE_INTERCOM_STOP_STATE &targetprocessorport
    LOCAL &stopstate
    ENTRY &stopstate
    IF "&stopstate"!="RUNNING"
    (
        GOSUB FAILED_TO_BREAK
    )

    do std_utils DISABLE_CLOCK_GATING &image &targetprocessorport ENABLE
    
    //return cookie to original value
    WAIT.500ms //to ensure handler has time to read cookie before clearing it
    INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&INSTRUCTION_COOKIE_ADDRESS %LONG %LE 0x0
    
    RETURN SUCCESS
    

////////////////////////////////////////
//
//          WARMBOOTWAKE
//
//          Just sends a CTI pulse to Q6. 
//          Software on target should busywait for 10 seconds
//          Needs the ability to be run from a remote processor for TPIU calibration.
//
///////////////////////////////////////// 
WARMBOOTWAKE:
LOCAL &break
ENTRY &break
    
    PRINT "std_cti_mpss WARMBOOTWAKE: Disabling QuRT OSAM menu for warmboot wakeup"
    TASK.RESET
    
    WINPOS 75. 0. 40. 20. 0. 0. 
    AREA

    //attach to bootprocessor and load hwio
    GOSUB MAKE_SURE_BOOT_PROCESSOR_ATTACHED
    
    do std_intercom_do &targetprocessorport hwio
    
    INTERCOM.EVALUATE &targetprocessorport SYSTEM.MODE()
        &temp=EVAL()
    IF ("&temp"!="0x0B")
    (
        INTERCOM.EXECUTE &targetprocessorport SYSTEM.MODE.PREPARE
    )

    do std_utils DISABLE_CLOCK_GATING &image &targetprocessorport
    
    INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&DEBUG_COOKIE_ADDRESS %LONG %LE &DEBUG_COOKIE_VALUE
    INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&INSTRUCTION_COOKIE_ADDRESS %LONG %LE &INSTRUCTION_COOKIE_BUSYWAIT
    
    do std_intercom_do &targetprocessorport std_utils HWIO_OUTF SSC_QDSP6SS_DBG_CFG DBG_SW_REG &warmboot_dbg_value
    

    wait.200ms    
    GOSUB SEND_CTI_PULSE
    


    //if we're not already attached, attach.
    IF !(SYSTEM.MODE()==0x0B)
    (
        GOSUB ATTACH_TO_PROCESSOR
    )
    IF !(SYSTEM.MODE()==0x0B)
    (
        RETURN FAILURE
    )
    ELSE
    (
        IF STATE.RUN()==TRUE()
        (
            RETURN FAILURE
        )
        ELSE
        (
            RETURN SUCCESS
        )
    )
    
    RETURN //returns success
////////////////////////////////////////
//
//          CRASHPROCESSOR
//
//          Sets cookie value to crash.
//          Upon receiving CTI Interrupt, 
//          Q6 should call inject err fatal.
//
///////////////////////////////////////// 
CRASHPROCESSOR:
LOCAL &break
ENTRY &break

    WINPOS 75. 0. 40. 20. 0. 0. 
    AREA


    GOSUB MAKE_SURE_BOOT_PROCESSOR_ATTACHED
    
    do std_intercom_do &targetprocessorport hwio
    
    INTERCOM.EVALUATE &targetprocessorport SYSTEM.MODE()
        &temp=EVAL()
    IF ("&temp"!="0x0B")
    (
        INTERCOM.EXECUTE &targetprocessorport SYSTEM.MODE.PREPARE
    )
    INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&DEBUG_COOKIE_ADDRESS %LONG %LE &DEBUG_COOKIE_VALUE
    INTERCOM.EXECUTE &targetprocessorport D.S E&ACCESS:&INSTRUCTION_COOKIE_ADDRESS %LONG %LE &INSTRUCTION_COOKIE_CRASH


    wait.200ms
    GOSUB SEND_CTI_PULSE
    

    RETURN SUCCESS //returns success
    
  
    
////////////////////////////////////////
//
//          SETCOREASCTIMASTER
//  
//          Sets up this core as master on CTI network, 
//
///////////////////////////////////////// 
SETCOREASCTIMASTER:

        GOSUB MAKE_SURE_BOOT_PROCESSOR_ATTACHED
        do std_intercom_do &bootprocessorport do std_cti masters=&targetprocessor slaves=allapps,aotimer extraoption=use_level_trigger

    
GOTO EXIT
////////////////////////////////////////
//
//          SENDCTIBREAKTOALLCORES
//
//          Sets up this core as master on CTI network, 
//          then sends a pulse to break
//
///////////////////////////////////////// 
SENDCTIBREAKTOALLCORES:

    GOSUB MAKE_SURE_BOOT_PROCESSOR_ATTACHED
    
    do std_intercom_do &bootprocessorport std_cti masters=&targetprocessor slaves=allcores,aotimer extraoption=use_level_trigger
    do std_intercom_do &bootprocessorport std_cti TRIGSETCHANNEL &targetprocessor 0

    wait.500ms
    do std_intercom_do &bootprocessorport std_cti TRIGCLEARCHANNEL &targetprocessor 0

RETURN


////////////////////////////////////////
//
//          SENDCTIGOTOALLCORES
//          
//          Sets up this core as master on CTI network
//          Then sends a pulse to go.
//          This core must be halted
//
///////////////////////////////////////// 
SENDCTIGOTOALLCORES:

    GOSUB MAKE_SURE_BOOT_PROCESSOR_ATTACHED
    do std_intercom_do &bootprocessorport std_cti_apps SENDCTIGOTOALLCORES slpi
    RETURN 
   
  
////////////////////////////////////////
//
//      CTI_GUI
//          
//      Print GUI for CIT functions
//
///////////////////////////////////////// 
CTI_GUI:

    WINPOS 80. 0. 47. 14.
    
    DIALOG
    (
        HEADER "CTI MENU"
        //POS width, height, length
        POS 1. 0. 35.
         TEXT "Use the below button to resume the core. "
        POS 1. 1. 35.
         TEXT "This ensures that other processors resume as well."
        POS 1. 2. 45.
