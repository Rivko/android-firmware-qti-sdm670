LOCAL &COOKIE_BUSYWAIT &COOKIE_BKRPT &COOKIE_CRASH &MODEM_SELECT_MASK &ADSP_SELECT_MASK &PROCESSOR_SELECT_MASK
LOCAL &PROC_SELECT_SHIFT_VALUE
IF "&CHIPSET"!="sdm845"
(
    &COOKIE_BUSYWAIT=0x03444248
    &COOKIE_BKRPT=0x03444249
    &COOKIE_CRASH=0x0344424A
    &MPSS_SELECT_MASK=0X2
    &ADSP_SELECT_MASK=0X4
    &SLPI_SELECT_MASK=0X2

    &DEBUG_COOKIE_VALUE="0x53444247"

    &PROCESSOR_SELECT_MASK=&SLPI_SELECT_MASK
    &PROC_SELECT_SHIFT_VALUE=28.
    &INSTRUCTION_COOKIE_BUSYWAIT=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_BUSYWAIT
    &INSTRUCTION_COOKIE_BUSYWAIT="&INSTRUCTION_COOKIE_BUSYWAIT"
    &INSTRUCTION_COOKIE_BKRPT=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_BKRPT
    &INSTRUCTION_COOKIE_BKRPT="&INSTRUCTION_COOKIE_BKRPT"
    &INSTRUCTION_COOKIE_CRASH=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_CRASH
    &INSTRUCTION_COOKIE_CRASH="&INSTRUCTION_COOKIE_CRASH"
)
ELSE
(
    &COOKIE_BUSYWAIT=0x00444248
    &COOKIE_BKRPT=0x00444249
    &COOKIE_CRASH=0x0044424A
    
    &MPSS_SELECT_MASK=0X1
    &ADSP_SELECT_MASK=0X2
    &CDSP_SELECT_MASK=0X3
    &SLPI_SELECT_MASK=0X4
    &SSCM3_SELECT_MASK=0X5
    &SHRM_SELECT_MASK=0X6

    &DEBUG_COOKIE_VALUE="0x53444247"

    &PROCESSOR_SELECT_MASK=&SLPI_SELECT_MASK
    
    &PROC_SELECT_SHIFT_VALUE=24.
    &INSTRUCTION_COOKIE_BUSYWAIT=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_BUSYWAIT
    &INSTRUCTION_COOKIE_BUSYWAIT="&INSTRUCTION_COOKIE_BUSYWAIT"
    &INSTRUCTION_COOKIE_BKRPT=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_BKRPT
    &INSTRUCTION_COOKIE_BKRPT="&INSTRUCTION_COOKIE_BKRPT"
    &INSTRUCTION_COOKIE_CRASH=&PROCESSOR_SELECT_MASK<<&PROC_SELECT_SHIFT_VALUE+&COOKIE_CRASH
    &INSTRUCTION_COOKIE_CRASH="&INSTRUCTION_COOKIE_CRASH"
)

LOCAL &dbg_register_value &warmboot_dbg_value
&dbg_register_value=0x20
&warmboot_dbg_value=0x08
&warmboot_dbg_value=&dbg_register_value+&warmboot_dbg_value //value needs to be 0x28

LOCAL &FAILURE &SUCCESS
&FAILURE="failure"
&SUCCESS="success"

