;=============================================================================
; SUB: GetClockStatus
;=============================================================================

GetClockStatus:

  ENTRY &pmicIndex &periphTypeData &periphSubTypeData &periphBaseAddr

  &periphStatus=&PM_ERR_SUCCESS
  &enableStatus="NONE"
  &swEnableStatus="NONE"
  &pinCtrlStatus="NONE"
  &customStatus="NONE"
  &statusAddr=&periphBaseAddr+0x08
  &divCtlAddr=&periphBaseAddr+0x43
  &drvCtlAddr=&periphBaseAddr+0x44
  &enCtlAddr=&periphBaseAddr+0x46
  &pinCtlAddr=&periphBaseAddr+0x47
  &xoAdjAddr=&periphBaseAddr+0x5C
  &data=0

  IF ((&periphTypeData==0)||(&periphSubTypeData==0))
  (
    &periphStatus=&PM_ERR_FAILURE
  )
  ELSE IF (&periphSubTypeData==0xE)
  (
    &periphStatus=&PM_ERR_SUCCESS
  )
  ELSE
  (
    GOSUB PMICRead &statusAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)==0)
      (
        &enableStatus="OFF"
      )
      ELSE
      (
        &enableStatus="ON"
      )

      IF (&periphSubTypeData==0xC)
      (
        IF ((&data&0x07)==0)
        (
          &customStatus="SlpClkSrc:32KHzXO"
        )
        ELSE IF ((&data&0x07)==0x1)
        (
          &customStatus="SlpClkSrc:Ext32KHzClk"
        )
        ELSE IF ((&data&0x07)==0x2)
        (
          &customStatus="SlpClkSrc:CalRC"
        )
        ELSE IF ((&data&0x07)==0x3)
        (
          &customStatus="SlpClkSrc:32KHzLFRC"
        )
        ELSE IF ((&data&0x07)==0x4)
        (
          &customStatus="SlpClkSrc:19.2MHzRC"
        )
        ELSE IF ((&data&0x07)==0x5)
        (
          &customStatus="SlpClkSrc:SysXO"
        )
        ELSE
        (
          &customStatus="NONE"
        )
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &enCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)!=0)
      (
        &swEnableStatus="ON"
      )
      ELSE
      (
        &swEnableStatus="OFF"
      )

      IF ((&periphSubTypeData==0x8)||(&periphSubTypeData==0x9)||(&periphSubTypeData==0x2)||(&periphSubTypeData==0xB))
      (
        IF ((&data&0x01)!=0)
        (
          &pinCtrlStatus="ON"
        )
        ELSE
        (
          &pinCtrlStatus="OFF"
        )
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &pinCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&periphSubTypeData==0x8)||(&periphSubTypeData==0x9))
      (
        IF ((&data&0x01)!=0)
        (
          &pinCtrlStatus="ON"
        )
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    IF ((&periphSubTypeData==0x8)||(&periphSubTypeData==0x9)||(&periphSubTypeData==0xA))
    (
      GOSUB PMICRead &drvCtlAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        &data=(&data&0x03)
        IF (&data==0)
        (
          &customStatus="DrvStr:1X"
        )
        ELSE IF (&data==0x1)
        (
          &customStatus="DrvStr:2X"
        )
        ELSE IF (&data==0x2)
        (
          &customStatus="DrvStr:3X"
        )
        ELSE IF (&data==0x3)
        (
          &customStatus="DrvStr:4X"
        )
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )
    ELSE IF (&periphSubTypeData==0xB)
    (
      GOSUB PMICRead &divCtlAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        &data=(&data&0x07)
        IF (&data==0)
        (
          &customStatus="DivFactor:XO/1"
        )
        ELSE IF (&data==0x1)
        (
          &customStatus="DivFactor:XO/1"
        )
        ELSE IF (&data==0x2)
        (
          &customStatus="DivFactor:XO/2"
        )
        ELSE IF (&data==0x3)
        (
          &customStatus="DivFactor:XO/4"
        )
        ELSE IF (&data==0x4)
        (
          &customStatus="DivFactor:XO/8"
        )
        ELSE IF (&data==0x5)
        (
          &customStatus="DivFactor:XO/16"
        )
        ELSE IF (&data==0x6)
        (
          &customStatus="DivFactor:XO/32"
        )
        ELSE IF (&data==0x7)
        (
          &customStatus="DivFactor:XO/64"
        )
        ELSE
        (
          &customStatus="NONE"
        )
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )
    ELSE IF ((&periphSubTypeData==0x1)||(&periphSubTypeData==0x3))
    (
      GOSUB PMICRead &xoAdjAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        &data=(&data&0x3F)
        &customStatus="XO_TRIM:&data"
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )
  )

  RETURN &periphStatus &enableStatus &swEnableStatus &pinCtrlStatus &customStatus

;=============================================================================
; SUB: GetPowerRailStatus
;=============================================================================

GetPowerRailStatus:

  ENTRY &pmicIndex &periphTypeData &periphSubTypeData &periphBaseAddr

  &periphStatus=&PM_ERR_SUCCESS
  &enableStatus="NONE"
  &swEnableStatus="NONE"
  &pinCtrlStatus="NONE"
  &voltStatus="NONE"
  &vsetValidStatus="NONE"
  &modePrimStatus="NONE"
  &modeSecStatus="NONE"
  &customStatus="NONE"
  &verAddr=&periphBaseAddr+0x01
  &statusAddr=&periphBaseAddr+0x08
  &volCtl1Addr=&periphBaseAddr+0x40
  &volCtl2Addr=&periphBaseAddr+0x41
  &pinVoltAddr=&periphBaseAddr+0x42
  &vsetLbAddr=&periphBaseAddr+0x40
  &vsetUbAddr=&periphBaseAddr+0x41
  &vsetValidLbAddr=&periphBaseAddr+0x42
  &vsetValidUbAddr=&periphBaseAddr+0x43
  &modePrimAddr=&periphBaseAddr+0x45
  &modeSecAddr=&periphBaseAddr+0x49
  &enCtlAddr=&periphBaseAddr+0x46
  &pinCtlAddr=&periphBaseAddr+0x47
  &freqClkAddr=&periphBaseAddr+0x200+0x50
  &data=0
  &modeData=0
  &pinData=0
  &range=0
  &vmin=0
  &vstep=0
  &vset=0
  &vsetLb=0
  &vsetUb=0
  &voltage=0
  &pinvoltage=0
  &freq=0
  &isPeriphUnifiedRegFlag=0
  &isPeriphNewLdoFlag=0
  &isPeriphNewBuckFlag=0

  IF ((&periphTypeData==0)||(&periphSubTypeData==0))
  (
    &periphStatus=&PM_ERR_FAILURE
  )
  ELSE
  (
    IF ((&periphTypeData==&PM_LDO_PERIPH_TYPE)&&(&periphSubTypeData>=0x30))
    (
      &isPeriphUnifiedRegFlag=0x1

      IF (&periphSubTypeData>=0x53)
      (
        &isPeriphNewLdoFlag=0x1
      )
    )
    ELSE IF ((&periphTypeData==&PM_SMPS_HF_PERIPH_TYPE)&&(&periphSubTypeData>=0xA))
    (
      &isPeriphUnifiedRegFlag=0x1
      &isPeriphNewBuckFlag=0x1

      IF (&periphSubTypeData==0xA)
      (
        GOSUB PMICRead &verAddr &pmicIndex
        ENTRY &status &data
        IF (&data<0x3)
        (
          &isPeriphNewBuckFlag=0x0
        )
      )
    )
    ELSE IF ((&periphTypeData==&PM_SMPS_FTS_PERIPH_TYPE)&&(&periphSubTypeData>=0xA))
    (
      &isPeriphUnifiedRegFlag=0x1
      IF (&periphSubTypeData>=0xB)
      (
        &isPeriphNewBuckFlag=0x1
      )
    )
    ELSE IF (&periphTypeData==&PM_BOB_PERIPH_TYPE)
    (
      &isPeriphUnifiedRegFlag=0x1
    )

    GOSUB PMICRead &statusAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)==0)
      (
        &enableStatus="OFF"
      )
      ELSE
      (
        &enableStatus="ON"
      )
      IF (&isPeriphUnifiedRegFlag==0x1)&&(&periphTypeData!=&PM_BOB_PERIPH_TYPE)
      (
        IF ((&isPeriphNewBuckFlag!=0x1)&&(&isPeriphNewLdoFlag!=0x1))
        (
          IF ((&data&0x10)!=0)
          (
            &customStatus="NPM_TRUE"
          )
          ELSE
          (
            &customStatus="NPM_FALSE"
          )
        )
        ELSE
        (
          IF ((&data&0x3)==0x3)
          (
            &customStatus="NPM_TRUE"
          )
          ELSE IF ((&data&0x3)==0x2)
          (
            &customStatus="LPM_TRUE"
          )
          ELSE IF ((&data&0x3)==0x1)
          (
            &customStatus="RET_TRUE"
          )
          ELSE
          (
            &customStatus="NONE"
          )
        )
      )
      ELSE IF (((&periphTypeData==&PM_LDO_PERIPH_TYPE)||(&periphTypeData==&PM_LDO_ULT_PERIPH_TYPE))&&(&periphSubTypeData!=0x10))
      (
        IF ((&data&0x02)!=0)
        (
          &customStatus="NPM_TRUE"
        )
        ELSE
        (
          &customStatus="NPM_FALSE"
        )
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &enCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)!=0)
      (
        &swEnableStatus="ON"
      )
      ELSE
      (
        &swEnableStatus="OFF"
      )

      IF (&isPeriphUnifiedRegFlag==0x1)&&(&periphTypeData!=&PM_BOB_PERIPH_TYPE)
      (
        GOSUB PMICRead &pinCtlAddr &pmicIndex
        ENTRY &status &pinData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )
      )
      ELSE
      (
        &pinData=&data
      )
      IF ((&pinData&0x0F)!=0)
      (
        &pinCtrlStatus="HW_EN("
        IF ((&pinData&0x01)!=0)
        (
          &pinCtrlStatus="&pinCtrlStatus"+"0,"
        )
        IF ((&pinData&0x02)!=0)
        (
          &pinCtrlStatus="&pinCtrlStatus"+"1,"
        )
        IF ((&pinData&0x04)!=0)
        (
          &pinCtrlStatus="&pinCtrlStatus"+"2,"
        )
        IF ((&pinData&0x08)!=0)
        (
          IF (&isPeriphUnifiedRegFlag==0x1)
          (
            &pinCtrlStatus="&pinCtrlStatus"+"AWAKE"
          )
          ELSE
          (
            &pinCtrlStatus="&pinCtrlStatus"+"3"
          )
        )
        &pinCtrlStatus="&pinCtrlStatus"+")"
      )
      ELSE
      (
        &pinCtrlStatus="OFF"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    IF (&periphTypeData!=&PM_BOOST_PERIPH_TYPE)
    (
      GOSUB PMICRead &modePrimAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        IF (&isPeriphUnifiedRegFlag==0x1)&&(&periphTypeData!=&PM_BOB_PERIPH_TYPE)
        (
          IF (&data==0x7)
          (
            &modePrimStatus="NPM"
          )
          ELSE IF ((&data==0x6)&&(&periphTypeData!=&PM_LDO_PERIPH_TYPE))
          (
            IF (&isPeriphNewBuckFlag!=0x1)
            (
              &modePrimStatus="AUTO"
            )
            ELSE
            (
              &modePrimStatus="AUTO_LPM"
            )
          )
          ELSE IF (&data==0x5)
          (
            IF ((&isPeriphNewBuckFlag!=0x1)&&(&isPeriphNewLdoFlag!=0x1))
            (
              &modePrimStatus="LPM"
            )
            ELSE IF (&isPeriphNewBuckFlag==0x1)
            (
              &modePrimStatus="AUTO_RM"
            )
            ELSE
            (
              &modePrimStatus="NPM(U)"
            )
          )
          ELSE IF (&data==0x4)
          (
            IF ((&isPeriphNewBuckFlag!=0x1)&&(&isPeriphNewLdoFlag!=0x1))
            (
              &modePrimStatus="RETENTION"
            )
            ELSE
            (
              &modePrimStatus="LPM"
            )
          )
          ELSE IF ((&data==0x3)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
          (
            &modePrimStatus="BYPASS"
          )
          ELSE IF ((&data==0x2)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
          (
            &modePrimStatus="ACT_BYPASS_NPM"
          )
          ELSE IF ((&data==0x1)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
          (
            &modePrimStatus="ACT_BYPASS_LPM"
          )
          ELSE
          (
            &modePrimStatus="NPM(U)"
          )

          GOSUB PMICRead &pinCtlAddr &pmicIndex
          ENTRY &status &pinData
          IF (&status==0)
          (
            &periphStatus=&PM_ERR_FAILURE
          )

          &pinData=&pinData>>4

          IF ((&pinData&0x0F)!=0)
          (
            GOSUB PMICRead &modeSecAddr &pmicIndex
            ENTRY &status &modeData
            IF (&status==0)
            (
              &periphStatus=&PM_ERR_FAILURE
            )
            IF (&modeData==0x7)
            (
              &modePrimStatus="NPM"
            )
            ELSE IF ((&modeData==0x6)&&(&periphTypeData!=&PM_LDO_PERIPH_TYPE))
            (
              IF (&isPeriphNewBuckFlag!=0x1)
              (
                &modePrimStatus="AUTO"
              )
              ELSE
              (
                &modePrimStatus="AUTO_LPM"
              )
            )
            ELSE IF (&modeData==0x5)
            (
              IF ((&isPeriphNewBuckFlag!=0x1)&&(&isPeriphNewLdoFlag!=0x1))
              (
                &modePrimStatus="LPM"
              )
              ELSE IF (&isPeriphNewBuckFlag==0x1)
              (
                &modePrimStatus="AUTO_RM"
              )
              ELSE
              (
                &modePrimStatus="NPM(U)"
              )
            )
            ELSE IF (&modeData==0x4)
            (
              IF ((&isPeriphNewBuckFlag!=0x1)&&(&isPeriphNewLdoFlag!=0x1))
              (
                &modePrimStatus="RET"
              )
              ELSE
              (
                &modePrimStatus="LPM"
              )
            )
            ELSE IF ((&modeData==0x3)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
            (
              &modeSecStatus="BYP"
            )
            ELSE IF ((&modeData==0x2)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
            (
              &modeSecStatus="ACTBYPN"
            )
            ELSE IF ((&modeData==0x1)&&(&periphTypeData==&PM_LDO_PERIPH_TYPE))
            (
              &modeSecStatus="ACTBYPL"
            )
            ELSE
            (
              &modeSecStatus="NPM"
            )
            &modeSecStatus="&modeSecStatus"+"_EN("
            IF ((&pinData&0x01)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"0,"
            )
            IF ((&pinData&0x02)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"1,"
            )
            IF ((&pinData&0x04)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"2,"
            )
            IF ((&pinData&0x08)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"AWAKE"
            )
            &modeSecStatus="&modeSecStatus"+")"
          )
        )
        ELSE IF ((&periphTypeData==&PM_LDO_PERIPH_TYPE)||(&periphTypeData==&PM_LDO_ULT_PERIPH_TYPE))
        (
          IF ((&data&0x20)!=0)
          (
            &modePrimStatus="BYPASS"
          )
          ELSE IF ((&data&0x80)!=0)
          (
            &modePrimStatus="NPM"
          )
          ELSE IF ((&data&0x10)!=0)
          (
            &modePrimStatus="FW_PMIC_AWAKE"
          )
          ELSE
          (
            &modePrimStatus="LPM"
          )

          IF ((&data&0x0F)!=0)
          (
            &modeSecStatus="NPM_HW_EN"
            IF ((&data&0x01)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"0,"
            )
            IF ((&data&0x02)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"1,"
            )
            IF ((&data&0x04)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"2,"
            )
            IF ((&data&0x08)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"3"
            )
          )
        )
        ELSE IF (&periphTypeData==&PM_BOOST_BYP_PERIPH_TYPE)
        (
          IF ((&data&0x80)!=0)
          (
            &modePrimStatus="AUTO_BOOST"
          )
          ELSE
          (
            &modePrimStatus="FORCED_BYPASS"
          )
        )
        ELSE IF (&periphTypeData==&PM_BOB_PERIPH_TYPE)
        (
          IF (&data==0x3)
          (
            &modePrimStatus="FPWM"
          )
          ELSE IF (&data==0x2)
          (
            &modePrimStatus="AUTO"
          )
          ELSE IF (&data==0x1)
          (
            &modePrimStatus="FPFM"
          )
          ELSE IF (&data==0x0)
          (
            &modePrimStatus="PASS"
          )
        )
        ELSE
        (
          IF ((&data&0x80)!=0)
          (
            &modePrimStatus="NPM"
          )
          ELSE IF ((&data&0x40)!=0)
          (
            &modePrimStatus="AUTO"
          )
          ELSE IF ((&data&0x10)!=0)
          (
            &modePrimStatus="FW_PMIC_AWAKE"
          )
          ELSE
          (
            &modePrimStatus="LPM"
          )

          IF ((&data&0x0F)!=0)
          (
            &modeSecStatus="NPM_FW_HW_EN"
            IF ((&data&0x01)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"0,"
            )
            IF ((&data&0x02)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"1,"
            )
            IF ((&data&0x04)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"2,"
            )
            IF ((&data&0x08)!=0)
            (
              &modeSecStatus="&modeSecStatus"+"3"
            )
          )
        )
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )

    IF (&isPeriphUnifiedRegFlag==0x1)
    (
      GOSUB PMICRead &vsetLbAddr &pmicIndex
      ENTRY &status &vsetLb
      GOSUB PMICRead &vsetUbAddr &pmicIndex
      ENTRY &status &vsetUb
      IF (&status!=0)
      (
        &vmin=0
        &vstep=1000.
        &vset=((&vsetUb<<8)|(&vsetLb))
        &voltage=&vmin+(&vset*&vstep)
        &voltStatus=FORMAT.DECIMAL(0,&voltage)
      )
      GOSUB PMICRead &vsetValidLbAddr &pmicIndex
      ENTRY &status &vsetLb
      GOSUB PMICRead &vsetValidUbAddr &pmicIndex
      ENTRY &status &vsetUb
      IF (&status!=0)
      (
        &vmin=0
        &vstep=1000.
        &vset=((&vsetUb<<8)|(&vsetLb))
        &voltage=&vmin+(&vset*&vstep)
        &vsetValidStatus=FORMAT.DECIMAL(0,&voltage)
      )
    )
    ELSE
    (
      GOSUB PMICRead &volCtl2Addr &pmicIndex
      ENTRY &status &vset
      IF (&status!=0)
      (
        &range=0

        IF (&periphTypeData==&PM_SMPS_HF_PERIPH_TYPE)
        (
          GOSUB PMICRead &volCtl1Addr &pmicIndex
          ENTRY &status &range
          IF ((&status!=0)&&(&range<=1))
          (
            &vmin=VAR.VALUE(\SmpsHfPeriphVminNamesArr[&range])
            &vstep=VAR.VALUE(\SmpsHfPeriphVstepNamesArr[&range])
            &voltage=&vmin+(&vset*&vstep)
            &voltStatus=FORMAT.DECIMAL(0,&voltage)
          )
        )
        ELSE IF (&periphTypeData==&PM_SMPS_FTS_PERIPH_TYPE)
        (
          GOSUB PMICRead &volCtl1Addr &pmicIndex
          ENTRY &status &range
          IF (&periphSubTypeData==0x8)
          (
            IF ((&status!=0)&&(&range<=1))
            (
              &vmin=VAR.VALUE(\SmpsFtsPeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\SmpsFtsPeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE IF (&periphSubTypeData==0x9)
          (
            IF ((&status!=0)&&(&range<=1))
            (
              &vmin=VAR.VALUE(\SmpsFts2p5PeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\SmpsFts2p5PeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE
          (
            &voltStatus="NONE"
          )
        )
        ELSE IF (&periphTypeData==&PM_SMPS_ULT_PERIPH_TYPE)
        (
          IF ((&periphSubTypeData==0xD)||(&periphSubTypeData==0xE)||(&periphSubTypeData==0xF))
          (
            IF ((&vset&0x60)==0x60)
            (
              &range=1
              &vset=&vset&0x1F
            )
            ELSE
            (
              &range=0
            )

            &vmin=VAR.VALUE(\SmpsUlt1PeriphVminNamesArr[&range])
            &vstep=VAR.VALUE(\SmpsUlt1PeriphVstepNamesArr[&range])
            &voltage=&vmin+(&vset*&vstep)
            &voltStatus=FORMAT.DECIMAL(0,&voltage)
          )
          ELSE IF (&periphSubTypeData==0x10)
          (
            &range=0
            &vmin=VAR.VALUE(\SmpsUlt4PeriphVminNamesArr[&range])
            &vstep=VAR.VALUE(\SmpsUlt4PeriphVstepNamesArr[&range])
            &voltage=&vmin+(&vset*&vstep)
            &voltStatus=FORMAT.DECIMAL(0,&voltage)
          )
          ELSE
          (
            &voltStatus="NONE"
          )
        )
        ELSE IF (&periphTypeData==&PM_BOOST_PERIPH_TYPE)
        (
          &range=0
          &vmin=VAR.VALUE(\BoostPeriphVminNamesArr[&range])
          &vstep=VAR.VALUE(\BoostPeriphVstepNamesArr[&range])
          &voltage=&vmin+(&vset*&vstep)
          &voltStatus=FORMAT.DECIMAL(0,&voltage)
        )
        ELSE IF (&periphTypeData==&PM_BOOST_BYP_PERIPH_TYPE)
        (
          &range=0
          &vmin=VAR.VALUE(\BoostBypPeriphVminNamesArr[&range])
          &vstep=VAR.VALUE(\BoostBypPeriphVstepNamesArr[&range])
          &voltage=&vmin+(&vset*&vstep)
          &voltStatus=FORMAT.DECIMAL(0,&voltage)
        )
        ELSE IF (&periphTypeData==&PM_LDO_PERIPH_TYPE)
        (
          GOSUB PMICRead &volCtl1Addr &pmicIndex
          ENTRY &status &range
          IF ((&periphSubTypeData==0x1)||(&periphSubTypeData==0x2)||(&periphSubTypeData==0x3))
          (
            IF ((&status!=0)&&(&range<=2))
            (
              &vmin=VAR.VALUE(\LdoNMOSPeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\LdoNMOSPeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE IF ((&periphSubTypeData==0x5)||(&periphSubTypeData==0x7))
          (
            IF ((&status!=0)&&(&range<=2))
            (
              &vmin=VAR.VALUE(\LdoN1200PeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\LdoN1200PeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE IF ((&periphSubTypeData==0x4)||(&periphSubTypeData==0x6))
          (
            IF ((&status!=0)&&(&range<=2))
            (
              &vmin=VAR.VALUE(\LdoN600PeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\LdoN600PeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE IF (((&periphSubTypeData>=0x8)&&(&periphSubTypeData<=0xC))||((&periphSubTypeData>=0x28)&&(&periphSubTypeData<=0x2C)))
          (
            IF ((&status!=0)&&(&range<=4))
            (
              &vmin=VAR.VALUE(\LdoPMOSPeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\LdoPMOSPeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE IF ((&periphSubTypeData==0x10)||(&periphSubTypeData==0x18))
          (
            IF ((&status!=0)&&(&range<=1))
            (
              &vmin=VAR.VALUE(\LdoLNPeriphVminNamesArr[&range])
              &vstep=VAR.VALUE(\LdoLNPeriphVstepNamesArr[&range])
              &voltage=&vmin+(&vset*&vstep)
              &voltStatus=FORMAT.DECIMAL(0,&voltage)
            )
          )
          ELSE
          (
            &voltStatus="NONE"
          )
        )
        ELSE IF (&periphTypeData==&PM_LDO_ULT_PERIPH_TYPE)
        (
          &range=0
          IF (((&periphSubTypeData>=0x1)&&(&periphSubTypeData<=0x7))||(&periphSubTypeData==0x15)||(&periphSubTypeData==0x14))
          (
            &vmin=VAR.VALUE(\LdoUltNMOSPeriphVminNamesArr[&range])
            &vstep=VAR.VALUE(\LdoUltNMOSPeriphVstepNamesArr[&range])
            &voltage=&vmin+(&vset*&vstep)
            &voltStatus=FORMAT.DECIMAL(0,&voltage)
          )
          ELSE IF (((&periphSubTypeData>=0x8)&&(&periphSubTypeData<=0xC))||((&periphSubTypeData>=0x28)&&(&periphSubTypeData<=0x2D)))
          (
            &vmin=VAR.VALUE(\LdoUltPMOSPeriphVminNamesArr[&range])
            &vstep=VAR.VALUE(\LdoUltPMOSPeriphVstepNamesArr[&range])
            &voltage=&vmin+(&vset*&vstep)
            &voltStatus=FORMAT.DECIMAL(0,&voltage)
          )
          ELSE
          (
            &voltStatus="NONE"
          )
        )
        ELSE
        (
          &voltStatus="NONE"
        )
      )
      ELSE
      (
        &voltStatus="NONE"
      )
    )

    IF ((&periphTypeData==&PM_SMPS_HF_PERIPH_TYPE)||(&periphTypeData==&PM_SMPS_FTS_PERIPH_TYPE)||(&periphTypeData==&PM_SMPS_ULT_PERIPH_TYPE))
    (
      GOSUB PMICRead &freqClkAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        &data=(&data&0x1F)
        &freq=(19200.)/(&data+0x1)
        ;&customStatus="Freq:"+FORMAT.DECIMAL(0,&freq)+"KHz"
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )

    IF (&periphTypeData==&PM_BOOST_BYP_PERIPH_TYPE)
    (
      GOSUB PMICRead &pinVoltAddr &pmicIndex
      ENTRY &status &data
      IF (&status!=0)
      (
        &range=0
        &vmin=VAR.VALUE(\BoostBypPeriphVminNamesArr[&range])
        &vstep=VAR.VALUE(\BoostBypPeriphVstepNamesArr[&range])
        &pinvoltage=&vmin+(&data*&vstep)
        &customStatus="PinV:"+FORMAT.DECIMAL(0,&pinvoltage)+"uV"
      )
      ELSE
      (
        &periphStatus=&PM_ERR_FAILURE
      )
    )
  )

  RETURN &periphStatus &enableStatus &swEnableStatus &pinCtrlStatus &modePrimStatus &modeSecStatus &voltStatus &vsetValidStatus &customStatus

;=============================================================================
; SUB: GetGpioStatus
;=============================================================================

GetGpioStatus:

  ENTRY &pmicIndex &periphTypeData &periphSubTypeData &periphBaseAddr

  &periphStatus=&PM_ERR_SUCCESS
  &enableStatus="NONE"
  &swEnableStatus="NONE"
  &modeStatus="NONE"
  &modeConfigStatus="NONE"
  &inputValStatus="NONE"
  &vinStatus="NONE"
  &pullStatus="NONE"
  &outDrvStatus="NONE"
  &statusAddr=&periphBaseAddr+0x08
  &modeCtlAddr=&periphBaseAddr+0x40
  &vinCtlAddr=&periphBaseAddr+0x41
  &pullCtlAddr=&periphBaseAddr+0x42
  &inCtlAddr=&periphBaseAddr+0x43
  &outCtlAddr=&periphBaseAddr+0x44
  &outDrvAddr=&periphBaseAddr+0x45
  &enCtlAddr=&periphBaseAddr+0x46
  &anaCtlAddr=&periphBaseAddr+0x4A
  &data=0
  &modeData=0

  IF ((&periphTypeData==0)||(&periphSubTypeData==0))
  (
    &periphStatus=&PM_ERR_FAILURE
  )
  ELSE
  (
    GOSUB PMICRead &statusAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)==0)
      (
        &enableStatus="OFF"
      )
      ELSE
      (
        &enableStatus="ON"
      )

      IF ((&data&0x01)==0)
      (
        &inputValStatus="LOW"
      )
      ELSE
      (
        &inputValStatus="HIGH"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &enCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)!=0)
      (
        &swEnableStatus="ON"
      )
      ELSE
      (
        &swEnableStatus="OFF"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &modeCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF (&periphSubTypeData<0x10)
      (
        &modeData=&data
        &data=((&data&0x70)>>4)
      )

      IF (&data==0)
      (
        &modeStatus="DigitalInput"
      )
      ELSE IF (&data==0x1)
      (
        &modeStatus="DigitalOutput"
      )
      ELSE IF (&data==0x2)
      (
        &modeStatus="DigitalInp/Outp"
      )
      ELSE IF (&data==0x3)
      (
        &modeStatus="AnalogPassThru"
      )
      ELSE
      (
        &modeStatus="NONE"
      )

      IF ((&data==0x1)||(&data==0x2))
      (
        IF (&periphSubTypeData<0x10)
        (
          IF ((&modeData&0x0F)==0)
          (
            &modeConfigStatus="LOW"
          )
          ELSE IF ((&modeData&0x0F)==0x1)
          (
            &modeConfigStatus="HIGH"
          )
          ELSE IF ((&modeData&0x0F)==0x2)
          (
            &modeConfigStatus="PairedGPIO"
          )
          ELSE IF ((&modeData&0x0F)==0x3)
          (
            &modeConfigStatus="NotPairedGPIO"
          )
          ELSE IF ((&modeData&0x0F)==0x4)
          (
            &modeConfigStatus="SpecialFunction1"
          )
          ELSE IF ((&modeData&0x0F)==0x5)
          (
            &modeConfigStatus="NotSpecialFunction1"
          )
          ELSE IF ((&modeData&0x0F)==0x6)
          (
            &modeConfigStatus="SpecialFunction2"
          )
          ELSE IF ((&modeData&0x0F)==0x7)
          (
            &modeConfigStatus="NotSpecialFunction2"
          )
          ELSE IF ((&modeData&0x0F)==0x8)
          (
            &modeConfigStatus="DTEST1"
          )
          ELSE IF ((&modeData&0x0F)==0x9)
          (
            &modeConfigStatus="NotDTEST1"
          )
          ELSE IF ((&modeData&0x0F)==0xA)
          (
            &modeConfigStatus="DTEST2"
          )
          ELSE IF ((&modeData&0x0F)==0xB)
          (
            &modeConfigStatus="NotDTEST2"
          )
          ELSE IF ((&modeData&0x0F)==0xC)
          (
            &modeConfigStatus="DTEST3"
          )
          ELSE IF ((&modeData&0x0F)==0xD)
          (
            &modeConfigStatus="NotDTEST3"
          )
          ELSE IF ((&modeData&0x0F)==0xE)
          (
            &modeConfigStatus="DTEST4"
          )
          ELSE IF ((&modeData&0x0F)==0xF)
          (
            &modeConfigStatus="NotDTEST4"
          )
          ELSE
          (
            &modeConfigStatus="NONE"
          )
        )
        ELSE
        (
          GOSUB PMICRead &outCtlAddr &pmicIndex
          ENTRY &status &modeData
          IF (&status==0)
          (
            &periphStatus=&PM_ERR_FAILURE
          )

          IF ((&modeData&0xF)==0)
          (
            &modeConfigStatus="LOW"
          )
          ELSE IF ((&modeData&0xF)==0x1)
          (
            &modeConfigStatus="PairedGPIO"
          )
          ELSE IF ((&modeData&0xF)==0x2)
          (
            &modeConfigStatus="SpecialFunction1"
          )
          ELSE IF ((&modeData&0xF)==0x3)
          (
            &modeConfigStatus="SpecialFunction2"
          )
          ELSE IF ((&modeData&0xF)==0x4)
          (
            &modeConfigStatus="SpecialFunction3"
          )
          ELSE IF ((&modeData&0xF)==0x5)
          (
            &modeConfigStatus="SpecialFunction4"
          )
          ELSE IF ((&modeData&0xF)==0x6)
          (
            &modeConfigStatus="DTEST1"
          )
          ELSE IF ((&modeData&0xF)==0x7)
          (
            &modeConfigStatus="DTEST2"
          )
          ELSE IF ((&modeData&0xF)==0x8)
          (
            &modeConfigStatus="DTEST3"
          )
          ELSE IF ((&modeData&0xF)==0x9)
          (
            &modeConfigStatus="DTEST4"
          )
          ELSE
          (
            &modeConfigStatus="NONE"
          )

          IF ((&modeData&0x80)!=0)
          (
            &modeConfigStatus="Not"+"&modeConfigStatus"
          )
        )
      )
      ELSE IF (&data==0)
      (
        GOSUB PMICRead &inCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )

        IF (&periphSubTypeData<0x10)
        (
          IF ((&modeData&0x0F)!=0)
          (
            &modeConfigStatus="DTEST"
            IF ((&modeData&0x01)!=0)
            (
              &modeConfigStatus="&modeConfigStatus"+"1,"
            )
            IF ((&modeData&0x02)!=0)
            (
              &modeConfigStatus="&modeConfigStatus"+"2,"
            )
            IF ((&modeData&0x04)!=0)
            (
              &modeConfigStatus="&modeConfigStatus"+"3,"
            )
            IF ((&modeData&0x08)!=0)
            (
              &modeConfigStatus="&modeConfigStatus"+"4"
            )
          )
          ELSE
          (
            &modeConfigStatus="DTEST_DISABLED"
          )
        )
        ELSE
        (
          IF ((&modeData&0x80)!=0)
          (
            &modeData=(&modeData&0x7)+1
            &modeConfigStatus="DTEST"+FORMAT.DECIMAL(0,&modeData)
          )
          ELSE
          (
            &modeConfigStatus="DTEST_DISABLED"
          )
        )
      )
      ELSE IF (&data==0x3)
      (
        GOSUB PMICRead &anaCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )
        &modeData=&modeData+1
        &modeConfigStatus="ATEST"+FORMAT.DECIMAL(0,&modeData)
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &vinCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      &data=(&data&0x07)
      &vinStatus="VIN"+FORMAT.DECIMAL(0,&data)

    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &pullCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      &data=(&data&0x07)
      IF (&data==0)
      (
        &pullStatus="Up30uAconst"
      )
      ELSE IF (&data==0x1)
      (
        &pullStatus="Up1.5uA"
      )
      ELSE IF (&data==0x2)
      (
        &pullStatus="Up31.5uAconst"
      )
      ELSE IF (&data==0x3)
      (
        &pullStatus="Up1.5uA+30uABoost"
      )
      ELSE IF (&data==0x4)
      (
        &pullStatus="Down10uA"
      )
      ELSE IF (&data==0x5)
      (
        &pullStatus="NoPull"
      )
      ELSE
      (
        &pullStatus="NONE"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &outDrvAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      &data=&data&0x03

      IF (&data==0x1)
      (
        &outDrvStatus="LOW"
      )
      ELSE IF (&data==0x2)
      (
        &outDrvStatus="MEDIUM"
      )
      ELSE IF (&data==0x3)
      (
        &outDrvStatus="HIGH"
      )
      ELSE
      (
        &outDrvStatus="NONE"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )
  )

  RETURN &periphStatus &enableStatus &swEnableStatus &modeStatus &modeConfigStatus &inputValStatus &vinStatus &pullStatus &outDrvStatus


;=============================================================================
; SUB: GetMppStatus
;=============================================================================

GetMppStatus:

  ENTRY &pmicIndex &periphTypeData &periphSubTypeData &periphBaseAddr

  &periphStatus=&PM_ERR_SUCCESS
  &enableStatus="NONE"
  &swEnableStatus="NONE"
  &modeStatus="NONE"
  &modeConfigStatus="NONE"
  &inputValStatus="NONE"
  &vinStatus="NONE"
  &pullStatus="NONE"
  &statusAddr=&periphBaseAddr+0x08
  &modeCtlAddr=&periphBaseAddr+0x40
  &vinCtlAddr=&periphBaseAddr+0x41
  &pullCtlAddr=&periphBaseAddr+0x42
  &inCtlAddr=&periphBaseAddr+0x43
  &enCtlAddr=&periphBaseAddr+0x46
  &anaOutCtlAddr=&periphBaseAddr+0x48
  &anaInCtlAddr=&periphBaseAddr+0x4A
  &anaSinkCtlAddr=&periphBaseAddr+0x4C
  &data=0
  &modeData=0

  IF ((&periphTypeData==0)||(&periphSubTypeData==0))
  (
    &periphStatus=&PM_ERR_FAILURE
  )
  ELSE
  (
    GOSUB PMICRead &statusAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)==0)
      (
        &enableStatus="OFF"
      )
      ELSE
      (
        &enableStatus="ON"
      )

      IF ((&data&0x01)==0)
      (
        &inputValStatus="LOW"
      )
      ELSE
      (
        &inputValStatus="HIGH"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &enCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      IF ((&data&0x80)!=0)
      (
        &swEnableStatus="ON"
      )
      ELSE
      (
        &swEnableStatus="OFF"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &modeCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (

      &modeData=&data
      &data=((&data&0x70)>>4)

      IF (&data==0)
      (
        &modeStatus="DigitalInput"

        GOSUB PMICRead &inCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )

        IF ((&modeData&0x0F)!=0)
        (
          &modeConfigStatus="DTEST"
          IF ((&modeData&0x01)!=0)
          (
            &modeConfigStatus="&modeConfigStatus"+"1,"
          )
          IF ((&modeData&0x02)!=0)
          (
            &modeConfigStatus="&modeConfigStatus"+"2,"
          )
          IF ((&modeData&0x04)!=0)
          (
            &modeConfigStatus="&modeConfigStatus"+"3,"
          )
          IF ((&modeData&0x08)!=0)
          (
            &modeConfigStatus="&modeConfigStatus"+"4"
          )
        )
        ELSE
        (
          &modeConfigStatus="DTEST_DISABLED"
        )       
      )
      ELSE IF (&data==0x1)
      (
        &modeStatus="DigitalOutput"

        IF ((&modeData&0x0F)==0)
        (
          &modeConfigStatus="LOW"
        )
        ELSE IF ((&modeData&0x0F)==0x1)
        (
          &modeConfigStatus="HIGH"
        )
        ELSE IF ((&modeData&0x0F)==0x2)
        (
          &modeConfigStatus="PairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x3)
        (
          &modeConfigStatus="InvertedPairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x8)
        (
          &modeConfigStatus="DTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0x9)
        (
          &modeConfigStatus="InvertedDTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0xA)
        (
          &modeConfigStatus="DTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xB)
        (
          &modeConfigStatus="InvertedDTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xC)
        (
          &modeConfigStatus="DTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xD)
        (
          &modeConfigStatus="InvertedDTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xE)
        (
          &modeConfigStatus="DTEST4"
        )
        ELSE IF ((&modeData&0x0F)==0xF)
        (
          &modeConfigStatus="InvertedDTEST4"
        )
        ELSE
        (
          &modeConfigStatus="NONE"
        )
      )
      ELSE IF (&data==0x2)
      (
        &modeStatus="DigitalInput/Output"

        IF ((&modeData&0x0F)==0)
        (
          &modeConfigStatus="LOW"
        )
        ELSE IF ((&modeData&0x0F)==0x1)
        (
          &modeConfigStatus="HIGH"
        )
        ELSE IF ((&modeData&0x0F)==0x2)
        (
          &modeConfigStatus="PairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x3)
        (
          &modeConfigStatus="InvertedPairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x8)
        (
          &modeConfigStatus="DTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0x9)
        (
          &modeConfigStatus="InvertedDTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0xA)
        (
          &modeConfigStatus="DTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xB)
        (
          &modeConfigStatus="InvertedDTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xC)
        (
          &modeConfigStatus="DTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xD)
        (
          &modeConfigStatus="InvertedDTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xE)
        (
          &modeConfigStatus="DTEST4"
        )
        ELSE IF ((&modeData&0x0F)==0xF)
        (
          &modeConfigStatus="InvertedDTEST4"
        )
        ELSE
        (
          &modeConfigStatus="NONE"
        )
      )
      ELSE IF (&data==0x3)
      (
        &modeStatus="BidirectionalLogic"

        IF ((&modeData&0x0F)==0)
        (
          &modeConfigStatus="LOW"
        )
        ELSE IF ((&modeData&0x0F)==0x1)
        (
          &modeConfigStatus="HIGH"
        )
        ELSE IF ((&modeData&0x0F)==0x2)
        (
          &modeConfigStatus="PairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x3)
        (
          &modeConfigStatus="InvertedPairedMPP"
        )
        ELSE IF ((&modeData&0x0F)==0x8)
        (
          &modeConfigStatus="DTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0x9)
        (
          &modeConfigStatus="InvertedDTEST1"
        )
        ELSE IF ((&modeData&0x0F)==0xA)
        (
          &modeConfigStatus="DTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xB)
        (
          &modeConfigStatus="InvertedDTEST2"
        )
        ELSE IF ((&modeData&0x0F)==0xC)
        (
          &modeConfigStatus="DTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xD)
        (
          &modeConfigStatus="InvertedDTEST3"
        )
        ELSE IF ((&modeData&0x0F)==0xE)
        (
          &modeConfigStatus="DTEST4"
        )
        ELSE IF ((&modeData&0x0F)==0xF)
        (
          &modeConfigStatus="InvertedDTEST4"
        )
        ELSE
        (
          &modeConfigStatus="NONE"
        )
      )
      ELSE IF (&data==0x4)
      (
        &modeStatus="AnalogInput"

        GOSUB PMICRead &anaInCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )

        IF (&modeData==0)
        (
          &modeConfigStatus="HKADC5"
        )
        ELSE IF (&modeData==1)
        (
          &modeConfigStatus="HKADC6"
        )
        ELSE IF (&modeData==2)
        (
          &modeConfigStatus="HKADC7"
        )
        ELSE IF (&modeData==3)
        (
          &modeConfigStatus="HKADC8"
        )
        ELSE IF (&modeData==4)
        (
          &modeConfigStatus="ATEST1"
        )
        ELSE IF (&modeData==5)
        (
          &modeConfigStatus="ATEST2"
        )
        ELSE IF (&modeData==6)
        (
          &modeConfigStatus="ATEST3"
        )
        ELSE IF (&modeData==7)
        (
          &modeConfigStatus="ATEST4"
        )
      )
      ELSE IF (&data==0x5)
      (
        &modeStatus="AnalogOutput"

        GOSUB PMICRead &anaOutCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )

        IF (&modeData==0)
        (
          &modeConfigStatus="VREF_1V25"
        )
        ELSE IF (&modeData==1)
        (
          &modeConfigStatus="VREF_0V625"
        )
        ELSE IF (&modeData==2)
        (
          &modeConfigStatus="VREF_0V3125"
        )
        ELSE IF (&modeData==3)
        (
          &modeConfigStatus="PAIRED_MPP"
        )
        ELSE IF (&modeData==4)
        (
          &modeConfigStatus="ATEST1"
        )
        ELSE IF (&modeData==5)
        (
          &modeConfigStatus="ATEST2"
        )
        ELSE IF (&modeData==6)
        (
          &modeConfigStatus="ATEST3"
        )
        ELSE IF (&modeData==7)
        (
          &modeConfigStatus="ATEST4"
        )
      )
      ELSE IF (&data==0x6)
      (
        &modeStatus="CurrentSink"

        GOSUB PMICRead &anaSinkCtlAddr &pmicIndex
        ENTRY &status &modeData
        IF (&status==0)
        (
          &periphStatus=&PM_ERR_FAILURE
        )

        IF (&modeData==0)
        (
          &modeConfigStatus="CURRENT_5MA"
        )
        ELSE IF (&modeData==1)
        (
          &modeConfigStatus="CURRENT_10MA"
        )
        ELSE IF (&modeData==2)
        (
          &modeConfigStatus="CURRENT_15MA"
        )
        ELSE IF (&modeData==3)
        (
          &modeConfigStatus="CURRENT_20MA"
        )
        ELSE IF (&modeData==4)
        (
          &modeConfigStatus="CURRENT_25MA"
        )
        ELSE IF (&modeData==5)
        (
          &modeConfigStatus="CURRENT_30MA"
        )
        ELSE IF (&modeData==6)
        (
          &modeConfigStatus="CURRENT_35MA"
        )
        ELSE IF (&modeData==7)
        (
          &modeConfigStatus="CURRENT_40MA"
        )
      )
      ELSE
      (
        &modeStatus="NONE"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &vinCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      &data=(&data&0x07)

      IF (&data<0x4)
      (
        &vinStatus="VIN"+FORMAT.DECIMAL(0,&data)
      )
      ELSE
      (
        &vinStatus="VIN0"
      )

    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )

    GOSUB PMICRead &pullCtlAddr &pmicIndex
    ENTRY &status &data
    IF (&status!=0)
    (
      &data=(&data&0x07)
      IF (&data==0)
      (
        &pullStatus="0.6K"
      )
      ELSE IF (&data==0x1)
      (
        &pullStatus="Open(InfiniteZ)"
      )
      ELSE IF (&data==0x2)
      (
        &pullStatus="10K"
      )
      ELSE IF (&data==0x3)
      (
        &pullStatus="30K"
      )
      ELSE
      (
        &pullStatus="NONE"
      )
    )
    ELSE
    (
      &periphStatus=&PM_ERR_FAILURE
    )
  )

  RETURN &periphStatus &enableStatus &swEnableStatus &modeStatus &modeConfigStatus &inputValStatus &vinStatus &pullStatus

;=============================================================================
; SUB: SpmiCfgUpdateTarget
;=============================================================================

SpmiCfgUpdateTarget:

  &PM_SPMI_CFG_TARGET=DIALOG.STRING(SPMI_CFG_TARGET_PD)

  RETURN

;=============================================================================
; SUB: SetOwnerChannel
;=============================================================================

SetOwnerChannel:

  ENTRY &OwnerName

  IF &OwnerName=="APPS"
  (
    &SPMIOwnerChannel=0
  )
  ELSE IF &OwnerName=="TZ"
  (
    &SPMIOwnerChannel=1
  )
  ELSE IF &OwnerName=="MSS"
  (
    &SPMIOwnerChannel=2
  )
  ELSE IF &OwnerName=="DEFAULT"
  (
    &SPMIOwnerChannel=3
  )
  ELSE IF &OwnerName=="AOP"
  (
    &SPMIOwnerChannel=4
  )
  ELSE IF &OwnerName=="SSC"
  (
    &SPMIOwnerChannel=5
  )

  RETURN

;=============================================================================
; SUB: PMICToSPMI
;=============================================================================

PMICToSPMI:

  ENTRY &addr &devid
  LOCAL &spmi_sid &spmi_addr

  IF (!STATE.RUN()&&(SYSTEM.MODE()<=0x5))
  (
    sys.m.a
  )

  IF (&devid==0)
  (
    &spmi_sid=0
  )
  ELSE
  (
    IF (VAR.VALUE(\PmicSlaveIdArr[&devid])!=0)
    (
      &spmi_sid=VAR.VALUE(\PmicSlaveIdArr[&devid])
    )
    ELSE
    (
      &spmi_sid=12. ;invalid slave id for now
    )
  )

  &pmic_sid=&addr>>16.
  IF &pmic_sid==1
  (
    &spmi_sid=&spmi_sid+1
  )

  &spmi_addr=&addr&0xFFFF

  RETURN &spmi_sid &spmi_addr


;=============================================================================
; SUB: PMICRead
;=============================================================================

PMICRead:

  ENTRY &addr &devid

  local &spmi_target &spmi_status

  GOSUB PMICToSPMI &addr &devid
  ENTRY &spmi_sid &spmi_addr

  do &PmicIoScript read &SPMIOwnerChannel &BusAccessMode &spmi_sid &spmi_addr
  ENTRY &status &data

  IF (&status==0xFF)
  (
    &spmi_target="&PM_SPMI_CFG_TARGET"

    IF ("&spmi_target"!="")
    (
      do &PmicIoScript config &spmi_target
      entry &spmi_status
      IF (&spmi_status==1)
      (
        do &PmicIoScript read &SPMIOwnerChannel &BusAccessMode &spmi_sid &spmi_addr
        ENTRY &status &data
      )
    )
    ELSE
    (
      DIALOG.OK "Select TARGET to initialize spmi!"
    )
  )

  RETURN &status &data

;=============================================================================
; SUB: PMICWrite
;=============================================================================

PMICWrite:

  ENTRY &addr &devid &data

  local &spmi_target &spmi_status

  GOSUB PMICToSPMI &addr &devid
  ENTRY &spmi_sid &spmi_addr

  do &PmicIoScript write &SPMIOwnerChannel &BusAccessMode &spmi_sid &spmi_addr &data
  ENTRY &status

  IF (&status==0xFF)
  (
    &spmi_target="&PM_SPMI_CFG_TARGET"

    IF ("&spmi_target"!="")
    (
      do &PmicIoScript config &spmi_target
      entry &spmi_status
      IF (&spmi_status==1)
      (
        do &PmicIoScript write &SPMIOwnerChannel &BusAccessMode &spmi_sid &spmi_addr &data
        ENTRY &status
      )
    )
    ELSE
    (
      DIALOG.OK "Select TARGET to initialize spmi!"
    )
  )

  RETURN &status


