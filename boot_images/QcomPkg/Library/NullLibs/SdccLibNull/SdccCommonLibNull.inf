[Defines]
  INF_VERSION                    = 0x00010005
  BASE_NAME                      = SdccCommonLib
  FILE_GUID                      = 0356B333-44AC-6714-2007-E0A9A0C6C5D0 
  MODULE_TYPE                    = BASE
  VERSION_STRING                 = 1.0
